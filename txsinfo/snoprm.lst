  1300						SUBTTL	Macro Definitions -- PPINIT - PUSH/POP MACRO INITIALIZATION
  1301
  1302
  1303					;   PPINIT IS CALLED BY EACH OF PUSHSA, PUSHR, POPSA & POPR
  1304					;	IT ESTABLISHES 2 MACROS.  PPADDR RETURNS THE ADDRESS
  1305					;	OF THE AREA THAT IS TO BE PUSHED OR POPPED.
  1306					;	PPLENG RETURNS THE LENGTH OF THIS AREA.
  1307					;	TO MAKE THIS EASY THE FOLLOWING EQUATED SYMBOLS ARE DEFINED.
  1308
  1309					;	REGI==IREG
  1310					;	LREGI==1
  1311			000004			REGP==P1
  1312			000002			LREGP==2
  1313			000006			REGS==S1
  1314			000003			LREGS==3
  1315			000011			REGT==T1
  1316			000004			LREGT==4
  1317			000015			REGQ==Q1
  1318			000003			LREGQ==3
  1319
  1320						DEFINE	PPINIT(TYPE,CODE)<	
  1321								;; TYPE MUST BE EITHER REG OR SAVE
  1322								;; CODE IS ARG PASSED FROM ORIGINAL CALL
  1323								;; MAY CONTAIN LEADING BLANKS
  1324						IRPC	CODE<
  1325							IF2,<
  1326							   IFNDEF TYPE'CODE,<EXTERNAL TYPE'CODE>>
  1327							IFNB <CODE><DEFINE PPADDR<TYPE'CODE>
  1328								STOPI>>	
  1329								;; SO MUCH FOR THE DEF. OF PPADDR
  1330						PPL==0		;; WE WILL COMPUTE HERE THE LENGTH OF THE AREA
  1331						IRPC	CODE<
  1332							IFNB	<CODE><PPL==PPL+L'TYPE'CODE>>
  1333						DEFINE	PPLENG<MIDENT(\PPL)>	>
  1334
