   253						SUBTTL	ALTE - Allocate a table element
   254
   255						 ; HERE TO ALLOCATE A TABLE ELEMENT
   256						 ; CALLING SEQUENCE
   257						 ; 	PUSH TABLE ONTO STACK
   258						 ;	PUSH REFERENCE ELEMENT ONTO STACK
   259						 ;	JSP	TLINK,ALTE
   260						 ;	RETURN 
   261
   262						 ; ALTE WILL REPLACE THE 2 DESCRIPTORS BY A 
   263						 ; TRAPPED VARIABLE WHICH POINTS TO A TEBLOK
   264						 ; WHICH CONTAINS THE REFERENCE ELEMENT IN
   265						 ; THE TEREF FIELD AND THE TABLE IN THE TEVAL FIELD
   266
   267	400150'	200 00 0 00 400457'	ALTE::	MOVE	Z,[TEBLOK,,LTEBLK] ; SET ...
   268	400151'	202 00 0 02 000000 		MOVEM	Z,BTCODE&BLENG(SURF) ; UP ...
   269	400152'	402 00 0 02 000001 		CLEARM	GCFLD(SURF)	; INITIAL ...
   270	400153'	402 00 0 02 000006 		CLEARM	TELINK(SURF)	; VALUES
   271
   272	400154'	262 01 0 02 000003 		POP	STACK,TEREF+1(SURF) 	; INSERT 
   273	400155'	262 01 0 02 000002 		POP	STACK,TEREF(SURF) 	; REFERENCE ELEMENT
   274	400156'	262 01 0 02 000005 		POP	STACK,TEVAL+1(SURF) 	; TEMPORARILY ...
   275	400157'	262 01 0 02 000004 		POP	STACK,TEVAL(SURF)		; SAVE TABLE
   276
   277	400160'	200 12 0 00 000002 		MOVE	T2,SURF		; PREPARE ...
   278	400161'	505 12 0 00 034000 		HRLI	T2,TVAR		; RETURN ...
   279	400162'	261 01 0 00 000012 		PUSH	STACK,T2	; DESCRIP-
   280	400163'	261 01 0 00 400460'		PUSH	STACK,[TVTBL]	; TOR
   281
   282	400164'	271 02 0 00 000007 		ADDI	SURF,LTEBLK	; BUMP SURF
   283	400165'	313 02 0 00 000000*		CAMLE	SURF,CEIL##	; NEED A GC?
   284	400166'	265 15 0 00 000000*		JSP	QLINK,GCOL##	; YES
   285
   286	400167'	254 00 0 11 000000 		JRST	0(TLINK)	; RETURN
   287

