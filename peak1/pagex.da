!! title Rtn VReplc ;
! =========================================================================
=====================================+=====================================
=                                                                         =
=                                                                         =
=				Rtn VReplc				  =
=                                                                         =
=                                                                         =
=====================================+=====================================


				yamm (4-4)


==========================================================================;


  simple integer procedure VReplc (integer SVMPage, DVMPage, Prot, Count(1); boolean OtherFrame(false));
  begin "VReplc"
    own integer array
      Arg[0:1];
    integer
      Tmp;

    Arg[0] _
	(if (OtherFrame) then bit(1) else 0) lor
	    bit((Prot land !mask(2)), 7) lor
	    xwd((Count land !mask(10)), DVMPage);

    Arg[1] _ SVMPage;

    Tmp _ calli(Arg[0], -'64);
    return(if (!skip!) then 0 else Tmp);
  end "VReplc";


