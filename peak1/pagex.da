				yamm (4-5)


==========================================================================;


  simple boolean procedure ChMFP (integer Chan, FPage, VMPage, Count(1), Prot(1); boolean OtherFrame(false));
  begin "ChMFP"
    own integer array
      Arg[0:1];

    Arg[0] _
      (if (OtherFrame) then bit(1) else 0) lor
	  bit((Prot land !mask(2)), 7) lor
	  xwd((Count land !mask(10)), VMPage);

    Arg[1] _ FPage;

    chnior(Chan, Arg[0], '35);
    return(not !skip!);
  end "ChMFP";


