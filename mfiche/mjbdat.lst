    29				;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
    30				;;;   ALL NEW USER SYMBOLS MUST BE OF THE FORM:                ;;;
    31				;;;   .JBXXX   IF THE SYMBOL IS IN THE LOW SEGMENT             ;;;
    32				;;;   .JBHXX   IF THE SYMBOL IS IN THE HIGH SEGMENT            ;;;
    33				;;;							       ;;;
    34				;;;   THE OLD FORMS OF JOBXXX AND JOBHXX ARE LEGAL ONLY FOR    ;;;
    35				;;;   COMPATABILITY WITH OLD PROGRAMS AND WILL EVENTUALLY DIE  ;;;
    36				;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
    37
    38				;MACRO TO DEFINE SYMBOLS FOR USER USE
    39				;THESE CANNOT BE CHANGED WITHOUT INVALIDATING OLD SAVED FILES
    40
    41					DEFINE U(SYMBOL,VALUE,LENGTH)
    42					<IFE  U2,<IFNDEF UJOBDAT,< SYMBOL==VALUE>
    43						IFDEF UJOBDAT,<SYMBOL=VALUE>
    44					ENTRY SYMBOL
    45					LOC=VALUE+LENGTH>>
    46
    47				;MACRO TO DEFINE SUPPRESSED SYMBOLS FOR USER USE
    48
    49					DEFINE US(SYMBOL,VALUE,LENGTH)
    50					<IFE  U2,<	 SYMBOL==VALUE
    51					INTERN SYMBOL
    52					LOC=VALUE+LENGTH>>
    53
    54				;MACRO TO DEFINE SYMBOLS FOR USER USE OF BOTH JOB AND .JB FORMAT
    55				;SAME AS U
    56
    57					DEFINE UJOB(SYMBOL,VALUE,LENGTH)
    58					<IFE  U2,<	.JB'SYMBOL=VALUE
    59							JOB'SYMBOL=VALUE
    60					ENTRY .JB'SYMBOL,JOB'SYMBOL
    61					LOC=VALUE+LENGTH>>
    62
    63				;MACRO TO DEFINE SYMBOLS FOR USER USE OF BOTH JOB AND .JB FORMAT
    64				;SAME AS US
    65
    66					DEFINE USJOB(SYMBOL,VALUE,LENGTH)
    67					<IFE  U2,<	 .JB'SYMBOL==VALUE
    68							 JOB'SYMBOL==VALUE
    69					INTERN .JB'SYMBOL,JOB'SYMBOL
    70					LOC=VALUE+LENGTH>>
    71
