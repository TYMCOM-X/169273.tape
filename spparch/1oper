       BRU START; BRS 10
VERNO  ASC '$$10.0$$/'
       ASC 'BLOOD'
MSG    POPD    100B5,2
       STP MSAVA; STX MSAVX
       EAX* 0; CXA; ETR EFFADR; LDB MINONE; LDX TOUT; BRS 34
       LDP MSAVA; LDX MSAVX; BRR 0
MSAVA  BSS 2
MSAVX  BSS 1
MSGP   POPD    101B5,2
       STA SAVA; STX SAVX
       EAX* 0; CXA; ETR EFFADR; SKN FNO; BRU MSGP1; BRM GPREL
       BRM PRMSG; SKN PSW1; BRM GCREL; BRU MSGP2
MSGP1  LDB MINONE; LDX FNO; BRS 34
MSGP2  LDA SAVA; LDX SAVX; BRR 0
SPACE  POPD    103B5,2
       STA SAVA; STX SAVX; EAX* 0; CXA; ETR EFFADR; CNA; CAX; CLA
       SKN FNO; BRU SPACE1; BRM PCHAR; BRX *-1; BRU MSGP2
SPACE1 CIO FNO; BRX *-1; BRU MSGP2
CR     POPD    104B5,2
       STA SAVA; STX SAVX; EAX* 0; CXA; ETR EFFADR; CNA; CAX; LDA CRET
       SKN FNO; BRU CR1; BRM PCHAR; BRX *-1; BRU MSGP2
CR1    CIO FNO; BRX *-1; BRU MSGP2
GPREL  ZRO; SKN PSW1; BRU *+2; BRR GPREL; STA TEMP; BRS 43; STP CREL
       LDP PREL; BRS 44; LDA TEMP; BRR GPREL
GCREL  ZRO; LDP CREL; BRS 44; BRR GCREL
