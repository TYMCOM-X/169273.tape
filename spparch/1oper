M25    ASC 'HUSH /'
M26    ASC 'RUB /'
M27    ASC 'CHG PSWD /'
M30    ASC '$$NAME         UNIV. UN   F.D.  DIST  STATUS/'
M31    ASC ' NO MULT. LOGINS /'
M33    ASC '$NO. OF OVERFLOW GROUPS: /'
M38    ASC '$INCORRECT REPLY TO YES OR NO INTEROGTORY'
       ASC '$IN COMANDS FILE-PROGRAM ABORTED$$/'
M40    ASC '$GLOBAL ACCT. NO. /'
M46    ASC '$ALL? (Y OR N) /'
M47    ASC '$DO YOU KNOW THE GLOBAL ACCT. NO.? (Y OR N) /'
M48    ASC '$ENTER GLOBAL ACCT NO. /'
M49    ASC '$ENTER A USER NAME BELONGING TO THIS'
       ASC '$GLOBAL ACCT. /'
       ASC '$NAME? (A,U,F, OR N) /'
M51    ASC '$ACCOUNT  NAME         USER NO. F.D. NO./'
M52    ASC '$USER NO. NAME         ACCOUNT  F.D. NO./'
M53    ASC '$F.D. NO. NAME         ACCOUNT  USER NO./'
M54    ASC '$NAME         ACCOUNT  USER NO. F.D. NO./'
M55    ASC '$ENTER F.D. NO. /'
M58    ASC '$ENTER F.D. NO. /'
M59    ASC ' IS UNASSIGNED$/'
M60    ASC 'OVERFLOW /'
M61    ASC 'F.D. NO. 1  /'
M62    ASC '$ANOTHER? /'
ERR1   ASC '$ERROR: TYPE USER NAME /'
ERR2   ASC '$PROGRAM ABORTED$/'
ERR7   ASC '$NAME NOT IN LUD/'
ERR8   ASC '$INVALID F.D. NO.$/'
ERR9   ASC '$LUD-F.D. INCONSISTENCY F.D. NO /'
ERR10  ASC '$CANNOT OPEN LUD FILE$/'
ERR11  ASC '$NOT VALID FOR DISTRICT PERSONNEL/'

USERS1 STA SOPT; BRM INIT; CR 1; BRM DATE; CR 1
       LDA O4000; STA LPTR; BRS 43
       STP NREL; SKN URELSW; BRU *+3; LDP UREL; BRU *+3
       ETR =77B6; CLB; BRS 44
USERS2 MIN BLNO; LDA BLNO; SKG D895; BRU *+2; BRU SORTU
       BRM GE1
USERS3 STX BLX
USERS4 LDA 0,2; SKG OH; BRU *+2; BRU USERS5; SKE MINTWO; BRU USERS2
       EAX 9,2; BRU USERS3
USERS5 LDA D4; STA CNT
USERS6 LDA 0,2; STA* LPTR; MIN LPTR; EAX 1,2; SKR CNT; BRU USERS6
       EAX 1,2; LDA 0,2; ETR ETRACT; STA* LPTR; MIN LPTR; EAX 1,2
       LDA 0,2; ETR EUNO; STA* LPTR; MIN LPTR; EAX 2,2
       STX BLX; LDA LPTR; SKG BUFTOP; BRU USERS4; HLT
GE1    ZRO; LDA BLNO; LRSH 23; DIV D128; STB MTEMP; CLB; LSH 13
       ADD BADR; ADD MTEMP; CAB; STB LDISC; LDA UBUFAD
       LDX O100; BRS 124
       EAX UBUF; BRR GE1
CLBUF  ZRO; CAB; LDA CRET; CIT CIN; BRU *+2; BRU *-2; CBA; BRR CLBUF
PONO   ZRO; STA SAVA1; STX SAVX1; LDB D8; SKN FNO; BRU *+2
       BRU PONO2; LDX FNO; BRS 36
PONO1  LDA SAVA1; LDX SAVX1; BRR PONO
PONO2  STP SA; BRM GPREL; LDP SA; BRM PNUM; SKN PSW1; BRM GCREL
       BRU PONO1
PDNO   ZRO; STA SAVA1; STX SAVX1; LDB D10; SKN FNO; BRU *+2
       BRU PDNO2; LDX FNO; BRS 36
PDNO1  LDA SAVA1; LDX SAVX1; BRR PDNO
PDNO2  STP SA; BRM GPREL; LDP SA; BRM PNUM; SKN PSW1; BRM GCREL
       BRU PDNO1
WRTSTG ZRO; MUL D3; LSH 23; SUB ONE; CAB; ADD D12; XAB; SKN FNO
       BRU *+2; BRU WSTG1; LDX FNO; BRS 35; BRR WRTSTG
WSTG1  BRM PSTG; BRR WRTSTG
SORTU  LDA LPTR; SUB ONE; STA ELPTR; LDA BUFBAS; BRM SETPT
       LDA SOPT; SKE O41; BRU *+2; BRU SAS; SKE O65; BRU *+2
       BRU SU; SKE O46; BRU SN; BRU SFI
SAS     MSGP M51
SAS1    LDA* APTR; SKG* APTR1; BRU *+2; BRU SAS5; BRM MPTR1; BRU SAS1
       CR 1; BRM PACCT; BRM WNAME; BRM PUN; SPACE 3
       BRM PFDNO; BRM SETPT1; BRU SAS1
SAS5    BRM EXCH; BRU SAS1
EXCH   ZRO; LDX MSEVEN; LDA* NPTR1,2; XMA* PTRE,2; STA* NPTR1,2
       BRX *-3; BRR EXCH
MPTR1  ZRO; LDA NPTR1; ADD D6; LDX MSEVEN; ADD ONE; STA PTRE,2
       BRX *-2
       LDA NPTR1; SKG ELPTR; BRR MPTR1; MIN MPTR1; BRR MPTR1
JON    ZRO
JON8   SKG OH; BRR JON; SKG 8SPA; SPACE 1; SKG 7SPA; SPACE 1
JON6   SKG 6SPA; SPACE 1; SKG 5SPA; SPACE 1
JON4   SKG 4SPA; SPACE 1; SKG 2SPA; SPACE 1; SKG D7
       SPACE 1; BRR JON
SETPT  ZRO; LDX MFOURT; ADD ONE; STA PTRE,2; BRX *-2; BRR SETPT
WNAME  ZRO; LDA NPTR; BRM WRTSTG; SPACE 1; BRR WNAME
PACCT  ZRO; LDA PACADR; STA JON; LDA* APTR
PACCT1 BRU JON6; BRM PONO; SPACE 3; BRR PACCT
PUN    ZRO; LDA PUNADR; STA JON; LDA* UPTR
PUN1   BRU JON6; BRM PONO; BRR PUN
PFDNO  ZRO; LDA PFDADR; STA JON; LDA* FDPTR
PFDNO1 BRU JON4; BRM PONO; BRR PFDNO
SETPT1 ZRO; LDA NPTR; ADD D6; BRM SETPT; LDA NPTR; SKG ELPTR
       BRR SETPT1; BRS 43; STP UREL; SKR URELSW; BRU *-1
       LDP NREL; BRS 44; BRU LLUD8
SU     MSGP M52
SU1    LDA* UPTR; SKG* UPTR1; BRU *+2; BRU SU5; BRM MPTR1; BRU SU1
       CR 1; BRM PUN; SPACE 3; BRM WNAME; BRM PACCT; BRM PFDNO
       BRM SETPT1; BRU SU1
SU5    BRM EXCH; BRU SU1
SFI    MSGP M53
SFI1   LDA* FDPTR; SKG* FDPTR1; BRU *+2; BRU SFI5; BRM MPTR1
       BRU SFI1; CR 1; BRM PFDNO; SPACE 5; BRM WNAME; BRM PACCT
       BRM PUN; BRM SETPT1; BRU SFI1
SFI5   BRM EXCH; BRU SFI1
SN     MSGP M54
SN1    LDA* NPTR; SKG* NPTR1; BRU *+2; BRU SN5
       SKE* NPTR1; BRU SN2; LDA* NPTRA; SKG* NPTRA1; BRU *+2; BRU SN5
       SKE* NPTRA1; BRU SN2; LDA* NPTRB; SKG* NPTRB1; BRU *+2
       BRU SN5; SKE* NPTRB1; BRU SN2; LDA* NPTRC; SKG* NPTRC1
       BRU SN2; BRU SN5
SN2    BRM MPTR1; BRU SN1; CR 1; BRM WNAME; BRM PACCT; BRM PUN
       SPACE 3; BRM PFDNO; BRM SETPT1; BRU SN1
SN5    BRM EXCH; BRU SN1
EUSE2  LDA CIN; SKE OH; BRU CINERR; BRU USERS+1
*PRINTER DRIVER
PRINT  ZRO; STA CHAR; BRU* WHERE
SPRINT LDX LPAADR; STX WHERE
        LDA D39; STA LC
LINE   SKN IN; BRU LINE1
       BRM PP; LDA NEGD39; STA PLC; LDA O24M1; STA TX
       CLA; LDX MEIGHT; STA COUNTE,2; BRX *-1
       LDA SPRADR; STA WHERE; BRR EPRINT
LINE1  SKR PLC; BRM PP
       LDA TX; STA BTX
       LDA NEGD32; STA WC; SKN TOPSW; BRM TOP; LDA ONE
       STA* TX; MIN TX
       LDA GOAADR; STA PCB
       SKR LC; BRU LPA
       BRU NOD
LPA    SKN CHAR; BRU *+2; BRR PRINT
       LDA MINONE; XMA CHAR
       SKG D63; BRU PCA
       SKE CRET; BRU *+2; BRU *+3; SKE LFEED; BRU NOB
       LDA O12; BRM PC; BRU *-2
NOB     SKE O135; BRU NOC; CLX; EAX NOB1; STX WHERE; BRR PRINT
NOB1   LDX LPAADR; STX WHERE
       CNA; CAX; LDA O12
       BRM PC; BRX *-2; BRU LPA
NOC    SKE O134; BRU PON
NOD    LDA NEG1; STA* BTX; LDA D38; STA LC; BRU LPA
PON    LDA O12; BRM PC; BRU LPA
PCA     CAX; LDA TAB,2; BRM PC; BRU LPA
PC     ZRO; CLB; BRR PCB
PCB   ZRO GOA-1; BRR PC
GOA   LSH 18; STA* TX; BRM PCB; LSH 12; ADM* TX
       BRM PCB; LSH 6; ADM* TX; BRM PCB; ADM* TX
       MIN TX; SKR WC; BRU LINE; BRM PCB; BRU GOA
PP     ZRO
       LDA TX; SUB O24M
       CAX; LDA O24M; BRS 106
       LDA BUF; SKA B77B5; BRU WHY
       LDA O24M1; STA TX; LDA NEGD38; STA PLC
       BRR PP
TOP    ZRO; LDA NEG1; SKR TOPSW; BRU *-1; MIN TOP
       BRR TOP


EPRINT ZRO; LDA MINONE; STA IN; LDA O12; BRM PC; BRU *-2

*PRINTS NUMBER TO ANY BASE - NUMBER IN A, BASE IN B
PNUM   ZRO; STA N2; STB N3; CLA
N6     STA N4; LDA N2
N7     STA N1; LRSH 23; DIV N3; SKE N4; BRU N7
       CBA; ADD O20; BRM PRINT; LDA N1; SKE N2; BRU N6; BRR PNUM

*PRINTS STRING
PSTG   ZRO; STP N1; GCI N1; BRR PSTG
       SKG 2SPA; BRU PSTG1; STA N3; BRM GPREL; LDA D6; BRM PRINT
       LDA N3; BRU PSGT2
PSTG1  STA N3; BRM GPREL; LDA N3
PSGT2  BRM PRINT; SKN PSW1; BRM GCREL; BRU PSTG+2

*PRINTS STANDARD MESSAGE
*EXPANDS CONTROL CHARS.
PRMSG  ZRO; MUL D3; LSH 23; SUB ONE; LDB MSGEND; STA N1; STB N2
PM4    GCI N1; HLT; SKE SLASH; BRU *+2; BRR PRMSG
       SKE D4; BRU PM7; LDA CRET
PM7    SKG 2SPA; BRU PM8; SKE CRET; BRU *+2; BRU PM8
       SKE LFEED; BRU *+2; BRU PM8
       STA N3; LDA D6; BRM PRINT; LDA N3; SUB O100
PM8    BRM PRINT; BRU PM4

WHY    LRSH 15; ETR 2SPA; SKE D7; HLT; MSG I7; BRS 10
I7     ASC '$PRINTER OUT OF READY/'

TAB    DATA 60B,71B,14B,17B,53B,57B,57B,14B
       DATA 74B,34B,54B,20B,73B,40B,33B,61B
       DATA 00B,01B,02B,03B,04B,05B,06B,07B
       DATA 10B,11B,15B,56B,36B,13B,16B,32B
       DATA 17B,21B,22B,23B,24B,25B,26B,27B
       DATA 30B,31B,41B,42B,43B,44B,45B,46B
       DATA 47B,50B,51B,62B,63B,64B,65B,66B
       DATA 67B,70B,71B,35B,76B,55B,57B,17B
       ZRO
IN     ZRO
WC   ZRO
BTX    ZRO
LC     ZRO
N1     ZRO
N2     ZRO
COUNTE EQU *
PLC    DATA 39+4B7
TX     DATA 24001B
BUF    EQU 24000B
CHAR   ZRO
WHERE  ZRO SPRINT
N3     ZRO
N4     ZRO

PCHAR  ZRO; SKN FNO; BRU PCHAR1; STP SA; STX SX; BRM GPREL
       BRM PRINT; SKN PSW1; BRM GCREL; LDP SA; LDX SX; BRR PCHAR
PCHAR1 CIO FNO; BRR PCHAR
SA     ZRO
SX     ZRO
GTNS1  ZRO; BRM CLBUF; LDA NAMPTA; STA NAMPT; STA NAMPT1; BRM GUNAM
       BRR GTNS1; MIN GTNS1; BRR GTNS1
GLUE1  ZRO; LDP NAMPT; STP PTNBF1; LDA =NAMBF1; STA POINT; BRM GENTRY
       BRR GLUE1; MIN GLUE1; BRR GLUE1

*OUTPUT DATE AND TIME TO AN OPENED FILE

DATE   ZRO; BRS 42; STX TLV5; STB TLV1; ABC; DIV =1800; ADD TLV1
       STA TLV11; LRSH 16; ADD ONE
       STA CURMON; SKN INITSW; BRU *+2; BRR DATE
       CR 1; BRM PDNO; LDA SLASH; BRM PCHAR
       LDA TLV11; ETR ETRMIN; RSH 23; DIV D1440; STB TLV11; ADD ONE
       BRM PDNO; LDA SLASH; BRM PCHAR; LDA TLV5; ADD D64; BRM PDNO
       SPACE 2; LDA TLV11; RSH 23; DIV D60; STB TLV11
       BRM PDNO; LDA COLON; BRM PCHAR; LDA TLV11; SKG D9; BRM ZERO
       BRM PDNO; CR 1; BRR DATE
ZERO   ZRO; STA SAVA; LDA O20; BRM PCHAR; LDA SAVA; BRR ZERO
*TYPE LIST OF VALID COMMANDS
HELP   BRM INTTEL; MSG M1 (VALID COMMANDS ARE)
       CR 1
       LDA =COMD; STA TLV1
HELP1  SKE =COMDE; BRU *+2; BRU HELP4
       LDX TLV1; SKN 0,2; BRU *+2; MSG MSTAR
       STX MSAVA; LDX MFOUR
       LDA* MSAVA; ETR =@4B7; STA LISBUF+4,2
       MIN MSAVA; BRX *-4; LDA =LISBUF; BRM WRTSTG; CR 1
       LDA TLV1; ADD D5; STA TLV1; BRU HELP1
MSTAR  ASC '* /'
HELP4  MSG SUBMSG; BRU BEGIN
SUBMSG ASC '$* INDICATES COMMAND HAS SUB COMMAND STRUCTURE AND WILL RESPOND'
       ASC '$WITH A >.  A SUB HELP COMMAND IS INCLUDED.$$/'

*LIST LUD

LLUD1A BRM DATE; CR 1
LLUD1B BRM GANT; SKR LLUDSW; BRU *-1; LDA =LISBUF; STA LPTR
LLUD1  LDA* LPTR; SKG OH; BRU LLUD8; SKR INITSW; BRU *-1; BRM MFDT
       MIN LPTR; BRU LLUD1
LLUD2  STX MTEMPX; LDA 6,2 (GAN)
       ETR ETRACT
       SKE* LPTR; BRU LLUDR; SKN RESTSW; BRU *+2; BRM CKREST
       SKN IMLSW; BRU *+2; BRU LLUD2A
       SKN INITSW; BRU LLUD2A; MSGP M40
       BRM PONO; CLA; STA INITSW; MSGP M30; CR 1
LLUD2A CXA; BRM WRTSTG; SPACE 3; LDX MTEMPX; SKN IMLSW; BRU *+2
       BRM LPGAN; SKN RESTSW; BRU *+2; BRU LLUD4
       LDA =LLUD3-1; STA JON; LDA 7,2; ETR EUNO
       BRU JON6
LLUD3  BRM PONO; SPACE 3; LDA =LLUD3A; STA JON; LDA 4,2
LLUD3A BRU JON4; BRM PONO; SPACE 3
       LDA 8,2; ETR 5SPA; LRSH 2; SKG D9; SPACE 1; SKG D99
       SPACE 1; BRM PDNO; SPACE 3
       SKN 6,2; BRU *+2; MSGP M10
       LDA 8,2; SKA =4B7; MSGP M21; SKA =2B6; MSGP M23; SKA =2B7
       MSGP M22; SKA =4B5; MSGP M31
       SKA =1B6; MSGP M25; SKA =1B4; MSGP M26
       SKA =4B4; BRU *+5; SKA =2B6; BRU LLUD4; MSGP M27; BRU LLUD4
       SKA =62B6; BRU LLUD4; MSGP M24
LLUD4  CR 1
LLUDR  LDX MTEMPX; BRU MFDT3
LLUD5  SKN CINSW; MSG M47; BRM CLBUF; BRM YN
       BRU LLUD6 (NO); SKN CINSW
       MSG M48; BRM CLBUF; BRM RONO
LLUD5A STA LISBUF; LDA =LISBUF; STA LPTR; BRM DATE; CR 1
       SKR INITSW; BRU *-1; SKR LLUDSW; BRU *-1
       BRM MFDT; BRU LLUD8
LLUD6  BRM GETGAN; BRU LLUD5A
GETGAN ZRO; SKN CINSW; MSG M49
LLUD7  BRM GTNS1
       BRU ELLUD2 (ERROR NAME STRING)
       LDA =NAMBF1; STA POINT; BRM GENTRY; BRU ELLUD3 (NAME NOT IN LUD)
       LDA 6,2; ETR ETRACT; BRR GETGAN

LPGAN  ZRO; LDA =LPGAN1; STA JON; LDA 6,2; ETR ETRACT
LPGAN1 BRU JON6; BRM PONO; SPACE 3; LDX MTEMPX; BRR LPGAN
CKREST ZRO; LDA 8,2; SKA =1B4; BRR CKREST; BRU LLUDR
LLUD8  SKN FNO; BRU BEGIN; LDA =38; STA MTEMP
       CR 1; SKR MTEMP; BRU *-2
LLUD8A ZRO 0 *CALLING ROUTINES TO LLUD8 MUST STORE RETURN HERE
ELLUD2 MSG ERR1; LDA CIN; SKE OH; BRU CINERR; BRM CLBUF1; LDX MINONE
       BRS 11; BRU LLUD7
ELLUD3 MSG ERR7; LDA CIN; SKE OH; BRU CINERR; BRU ELLUD2

*CLEARS SINGLE FILE DIRECTORY GROUP, IF ASSIGNED REPLACES NECESSARY
*FLAGS

CLFIL  BRM VER; SKN CINSW; MSG M55; BRM CLBUF; BRM RONO; STA FDPT
       SKG =5577B; SKG ONE; BRU ECLFIL (NOT VALID NO)
       BRM PGZERO; BRU ECLFIL
       BRM DIRR; BRM ZEROFD; LDA FDPT; SKG BUFBAS; BRU *+2
       BRU CLFIL2; STA TEMP; BRM CMFDT; LDX TEMP
       LDA FDTAB,2; SKE OH; BRU *+2; BRU CLFIL2
       LDA MINTWO; STA DBUF
CLFIL2 LDP OPNAM; STP DBUF+5; LDP OPNAM+2; STP DBUF+7
       BRM DIRW; BRU BEGIN
ECLFIL MSG ERR8; LDA CIN; SKE OH; BRU CINERR; BRU BEGIN



*PRINTS ALL OR SELECTED USERS FILES

FILES1 MIN FDPT; LDA FDPT; SKG BUFBAS; BRU *+2; BRU LLUD8
       BRM PGZERO; BRU FILES1
       BRM DIRR; LDA DBUF; SKE OH; BRU *+2; BRU FILES1; BRM PFILES
       CR 2; BRU FILES1
FSF1   SKN CINSW; MSG M58 (ENTER F.D. NO.)
       BRM CLBUF; BRM RONO; SKG BUFBAS; BRU *+2; BRU LLUD8; SKG OH
       BRU EFIL1 (INVALID NO.); BRM PGZERO; BRU EFIL1
       STA FDPT; LDA CRET; CIO TOUT; LDA FDPT
       BRM DIRR; LDA DBUF; SKE OH; BRU FSF2
       LDA FDPT; BRM TONO; MSG M59 (UNASSIGNED); BRU FSF3
FSF2   BRM PFILES
FSF3   SKN CINSW; MSG M62 (ANOTHER?); BRM CLBUF; BRM YN; BRU LLUD8
       BRU FSF1
NSFILE SKN CINSW; MSG M11; BRM GTNS1; BRU EFIL2 (ERROR NAME STRING)
       BRM GLUE1
       BRU EFIL2 (NAME NOT IN LUD)
       LDA 4,2; STA FDPT; LDA CRET; CIO TOUT; LDA FDPT
       BRM DIRR; SKG OH; BRU EFIL4
       BRM PFILES
NSF1   SKN CINSW; MSG M62; BRM CLBUF; BRM YN; BRU LLUD8
       BRM CLBUF1; BRU NSFILE
PFILES ZRO; BRM PFLIN1; BRU PFIL1
PFIL1  LDA FDPT; BRM PGZERO; BRR PFILES
PFIL2  STA GRP; BRM DIRR; LDA GRP; BRM PONO; LDA AOFLW; SKG OH
       BRU PFIL3; SPACE 2; BRM PONO
PFIL3  CR 1; LDX =DBUF
PFIL4  LDA 0,2; SKG OH; BRU PFIL5; STX TLV23; BRM PDIR; EAX 1,2
       SKN -1,2; BRU *-2; BRU PFIL4
PFIL5  LDA AOFLW; SKG OH; BRR PFILES; MSGP M60 (OVERFLOW )
       LDA POFLW; BRM PONO; CR 2; LDA AOFLW; BRU PFIL2
PFLIN1 ZRO; BRM CMFDT; LDX FDPT; CXA; SKG ONE; BRU PFL2
       SKN FDTAB,2; BRU *+2; BRR PFLIN1; LDA FDTAB,2
       SKG OH; BRU EFIL3; STA BLNO
       BRM GE1
PFL1   STX BLX; LDA 0,2; SKG OH; BRU *+2; BRU PFL1A; SKE MINTWO
       BRU PFL3
PFL1B  EAX 9,2; BRU PFL1
PFL1A  STX BLX; LDA 4,2; SKE FDPT; BRU PFL1B; CXA; BRM WRTSTG
       SPACE 1; SKN LICSW; BRU *+2; BRR PFLIN1
       LDX BLX; LDA 6,2; ETR ETRACT; BRM PONO
       SPACE 3; LDA 7,2; ETR EUNO; BRM PONO; SPACE 3; LDA 4,2
       BRM PONO; CR 2; BRR PFLIN1
PFL3   MIN BLNO; BRU PFL1-2
PFL2   MSGP M61 (US 1); SPACE 1; SKN LICSW; BRU *+2; BRR PFLIN1
       CR 2; BRR PFLIN1
PDIR   ZRO; BRM PO; BRM PO; BRM PO; BRM PO; SPACE 2
       LDA TLV23; MUL D3; LSH 23; SUB ONE; STA TLV2; LSH 6
       STA TLV21; BRU *+2
RFD    BRM OTCH; GCI TLV2; HLT; SKG =177B; BRU RFD; ETR =177B
       BRM OTCH; GCI TLV2; HLT; SKE =136B; BRU *+2; BRU RFD1
       BRM OTCH; GCI TLV2; HLT; SKE =136B; BRM OTCH
RFD1   CR 1; LDX TLV23; BRR PDIR
PO     ZRO; SPACE 1; LDA =PO1; STA JON; LDA* TLV23
PO1    BRU JON8; BRM PONO; MIN TLV23; BRR PO
OTCH   ZRO; SKG 2SPA; BRU PR2; ETR 2SPA; STA TLV1; LDA D6; BRM PCHAR
       LDA TLV1
PR2    BRM PCHAR; BRR OTCH
EFIL1  MSG ERR8; SKN CINSW; BRU *+2; BRU CINERR; BRU FSF3
EFIL2  MSG ERR1; SKN CINSW; BRU *+2; BRU CINERR; BRM CLBUF1
       BRU NSFILE+2
EFIL3  MSG ERR9; LDA FDPT; BRM TONO; LDA CRET; CIO TOUT; LDA FNO; SKG ONE
       BRR PFLIN1; MSGP ERR9; LDA FDPT; BRM PONO; CR 1; BRR PFLIN1
EFIL4  MSG ERR9; LDA FDPT; BRM TONO; LDA CRET; CIO TOUT; SKN CINSW
       BRU NSF1; BRU CINERR

*COUNTS REMAINING OVERFLOW GROUPS

GROUPS LDA BUFBAS; STA FDPT
GRO1   MIN FDPT; LDA FDPT; SKG =5577B; BRU *+2; BRU GRO2
       BRM DIRR; LDA DBUF; SKE OH; BRU GRO1; MIN TLV4; BRU GRO1
GRO2   MSG M33; LDA TLV4; BRM TDNO; LDA CRET; CIO TOUT; BRU BEGIN
INTTEL ZRO; LDA TOUT; STA FNO; BRR INTTEL
INIT   ZRO; LDP FILPTR; STP PTRFIL; SKN CINSW; MSG M2; BRM CLBUF
INIT1  CIO CIN; SKE CRET; BRU *+2; BRU INIT3; SKE =141B; BRU INIT2
       LDA MINONE; ADM PTRFIL+1; LDA 2SPA; CIO TOUT; BRU INIT1
INIT2  WCI PTRFIL; BRU INIT1
INIT3  LDP PTRFIL; SKSE PRINPT; BRU INIT4; LDA MINONE; STA FNO
       BRM WBUF; SKN PSW; BRU *+2; BRR INIT
       BRS 43; STP NREL; XAB; ETR =77007777B; XAB; BRS 44; LDA 24000B
       BRS 43; STP PREL; LDP NREL; BRS 44; SKR PSW; BRU *-1; BRR INIT
INIT4  LDX D3; LDA CIN; SKG OH; BRM INIT5; LDP PTRFIL; BRS 65
       BRU EINIT; STA FNO; SKG ONE; BRR INIT; LDA =17777777B
       SSP FNO; NOP; BRR INIT
INIT5  ZRO; XXA; CLB; LSH 12; MRG CIN; XXA; BRR INIT5
WBUF   ZRO; LDA D2; LDX MINONE; BRS 100; BRU *+2; BRR WBUF
       MSG NPRINT; BRS 10
NPRINT ASC '$UNABLE ASSIGN PRINTER$/'
EINIT  LDA CIN; SKE OH; BRU CINERR; BRU INIT+1
PRINPT DATA 3*PRIN-1,3*PRIN+6
PRIN   ASC 'PRINTER'

CLBUF1 ZRO; LDX MFOUR; CLA; STA NAMBF1+4,2; BRX *-1; BRR CLBUF1
VER    ZRO; SKN VERSW; BRU *+2; BRR VER
       BRS 71; BRU VER1A; SKR STASW; BRU *-1; SKR VERSW; BRU *-1
       SKR MGRSW; BRU *-1; BRU VER1B
VER1A  LDA ONE; CAX; BRS 141; LDP SFPTR; BRS 48; BRU ERV1; LDB =SFBUF
       LDX D4; BRS 96; LDA D4; SKA SFBUF+3; BRU *+2; BRU ERV2
       LDP SFPTR; BRS 64; BRU OPFER; STA SFIL
VER1AA CLA; SCP SFIL; HLT
VER1B  MSG VM1 (ENTER YOUR NAME PLEASE)
       LDA PTR1; STA PTR1+1; LDA =PTR1; LDB CRET; CLX; BRS 33
       SKN STASW; BRU *+2; BRR VER
       LDA PTR1+1; SUB PTR1; STA VT1
VER1   CIO SFIL; SKE =137B; BRU *+2; BRU NONAME
       SKE CRET; BRU VER1; CIT SFIL; BRU *+2; BRU NONAME
       LDA PTR2; STA PTR2+1; LDA =PTR2; LDB CRET
       LDX SFIL; BRS 33; LDA PTR2+1; SUB PTR2
       SKG OH; BRU NONAME; SKE VT1; BRU VER1
       LDP PTR2; SKSE PTR1; BRU VER1; GCI PTR1; HLT
       SKR PTR1; NOP; STA VXT8; BRM FONO
       SKA =1B7; BRU *+2; BRU ERV3; SKA =4B4; BRU *+2; BRU ERV3
       SKA =1B3; BRM SETMGR; SKA =4B6; BRU *+2; BRU VER1C
       SKR CSERSW; BRU *-1; CLA; STA DIST; BRU VER1C+2
VER1C  ETR 4SPA; STA DIST
       BRM FONO; STA VT1; BRM FONO; STA VT2; LDA SFIL; BRS 20
       LDX MINONE; LDA D3; BRS 12; MSG VM2 (ENTER YOUR PASSWORD PLEASE)
SGPS   LDP TERMWD; STP VXT1; STP VXT3; STB VXT5
       CLA; STA TLV1; LDA VXT8; ADM VXT3
GPS    LDA =20; STA TLV11
GPS1   LDX TLV1; LDA VXT1,2; MUL =6065B
       DIV =37777761B; CBA; ADD VXT2,2; CAB; ETR D3
       STA TLV1; LDA VXT1,2; XAB; STP VXT1,2; LDA VXT5
       XMA VXT1; STA VXT5; SKR TLV11
       BRU GPS1; CIO OH; SKE CRET; BRU *+2; BRU *+3
       ADM VXT1; BRU GPS
       LDX MINONE; LDA D2; BRS 12
       LDA VXT1; ADD VXT2; SKE VT1; BRU NOPASS; LDA VXT3; ADD VXT4
       SKE VT2; BRU NOPASS; SKR VERSW; BRU *-1; BRR VER
SETMGR ZRO; SKR MGRSW; BRU *-1; BRR SETMGR
MGRSW  ZRO
CSERSW ZRO
VERSW  ZRO

FONO   ZRO; LDB D8; LDX SFIL; BRS 38; BRR FONO

NOPASS MSG VME5; LDX MINONE; LDA D3; BRS 12; BRU SGPS

NONAME MSG VME6; LDA PTR1; STA PTR1+1; LDX =-20; CLA; STA OPNAM+20,2
       BRU VER1AA


TERMWD DATA 27657537B,55555555B

SFPTR  DATA 3*SF-1,3*SF+5
SF     ASC '()PSWD'
PTR1   DATA 3*OPNAM-1,3*OPNAM-1
OPNAM  BSS 20
PTR2   DATA 3*BUF2-1,3*BUF2-1
BUF2   BSS 20
SFBUF  BSS 20
VT1    ZRO
VT2    ZRO
VXT1   ZRO
VXT2   ZRO
VXT3   ZRO
VXT4   ZRO
VXT5   ZRO
VXT8   ZRO
SFIL   ZRO
ERV1   MSG VME1
ERV    MSG VME; BRS 10
ERV2   MSG VME2; BRU ERV
OPFER  MSG VME3; BRS 10
ERV3   MSG VME4; BRS 10

VME1   ASC '$NO PASSWORD FILE/'
VME    ASC '$NOTIFY DIVISION OPNS MGR AND SYSTEMS GROUP TECH DIV/'
VME2   ASC '$PASSWORD FILE STATUS LOST/'
VME3   ASC '$PASSWORD FILE BUSY-TRY LATER$/'
VME4   ASC '$UNVALIDATED NAME/'
VM1    ASC '$NAME /'
VM2    ASC '$PASSWORD /'
VME5   ASC '$ERROR, TYPE PASSWORD /'
VME6   ASC '$NAME DOES NOT APPEAR IN PASSWORD FILE$/'
RETFD  ZRO; LDA CALLER; LDX ONE; BRS 141; BRR RETFD

*MAKES TABLE OF FD POINTERS USED 

CMFDT  ZRO; SKN MFDTSW; BRU *+2; BRR CMFDT; BRM MFDT; SKR MFDTSW; BRU *-1
       BRR CMFDT
MFDTSW ZRO
MFDT  ZRO; LDA 34000B; LDA 30000B (GRAB PAGES)
       CLA; STA BLNO; BRM RPAGE
MFDT1 LDA BLNO; SKG D895; BRU *+2; BRU MFDT4
       BRM CKPAGE; BRU MFDT2
CKPAGE ZRO; LRSH 23; DIV D128; STB MTEMP; CLB; LSH 13; ADD BADR
       ADD MTEMP; SKG EPAGE; BRU *+2; BRM RPAGE; LDX BLX
       BRR CKPAGE
MFDT2 LDA 0,2; SKE OH; BRU *+2; BRU NBLK; SKE MINONE; BRU *+2
       BRU NBLK; SKE MINTWO; BRU *+2; BRU MFDT3
       SKN LISTSW; BRU *+2; BRU GANT1
       SKN LLUDSW; BRU *+2; BRU LLUD2
       SKN SLUDSW; BRU *+2; BRU SVLUD1
       SKN SPROSW; BRU *+2; BRU PROJP1
       LDA 4,2 (FD POINTER)
       SKG OH; HLT; COPY AX,XB; LDA BLNO; STA FDTAB,2
       CBX
MFDT3  EAX 9,2; BRU MFDT2
MFDT4  LDA =15; STA MTEMP; LDA =55555555B; LDX =400B; STX MCNT
       LDX MCNT; STA FDTAB,2; MIN MCNT; SKR MTEMP; BRU *-4
       STA FDTAB+1; BRR MFDT
NBLK   LDA D64; ADM BLX; MIN BLNO; BRU MFDT1

RPAGE  ZRO; LDA BLNO; LRSH 23; DIV D128; STB MTEMP; CLB; LSH 13
       ADD MTEMP; ADD BADR; CAB; LDA =LBUF
RPAGE1 LDX O4000; BRS 124; CBA; ADD O37; STA EPAGE; EAX LBUF
       STX BLX; BRR RPAGE
LBUF   EQU 34000B
FDTAB  EQU 30000B

*GET A USER NAME STRING, EXPECTS BUFFER POINTERS TO BE 
*NAMPT AND NAMPT1, NO SKIP ERROR RETURN WITH ERROR NUMBER IN X
*1=NULL STRING 2= NAME STRING OVER 12 CHARACTERS

GUNAM  ZRO; CLA; STA NAMCNT
GUNAM1 CIO CIN; SKE CRET; BRU *+2; BRU GUNAM4
       SKE =141B; BRU GUNAM2; GCD NAMPT; NOP; LDA 2SPA; CIO TOUT
       LDA NAMCNT; SKG OH; BRU GUNAM1; SUB ONE; STA NAMCNT; BRU GUNAM1
GUNAM2 SKG 2SPA (IGNORE CONTROL LETTERS); BRU *+2; BRU GUNAM1
       STA NAMTEM; LDA NAMCNT; SKE D12; BRU *+2; BRU GUNAM3
       LDA NAMTEM; WCI NAMPT; MIN NAMCNT; BRU GUNAM1
GUNAM3 CIO CIN; SKE CRET; BRU *-2; LDX D2; BRR GUNAM
GUNAM4 LDA NAMCNT; SKE OH; BRU *+3; LDX ONE; BRR GUNAM
       MIN GUNAM; BRR GUNAM

*CLOSE PROGRAM

THRU   MSG EOJ; BRS 10
EOJ    ASC '$$EOJ$$/'
*OUTPUT VERSION NUMBER

VERS   MSG VERNO; BRU BEGIN

*YES NO ROUTINE EXPECTS CALL TO BE IN FOLLOWING SEQUENCE
*MSG, BRM CLBUF, BRM YN  NO SKIP REPLY NO  SKIP REPLY YES

YN     ZRO; CAB
       CIO CIN; SKE ='   Y'; BRU YN1; CIO CIN; SKE CRET
       BRU *-2; CBA; MIN YN; BRR YN
YN1    SKE ='   N'; BRU YN3; CIO CIN; SKE CRET; BRU *-2
       CBA; BRR YN
YN3    LDA CIN; SKE OH
       BRU YNERR; LDX MINONE; BRS 11; LDA YN; SUB D3; STA YN; BRR YN
YNERR  MSG M38; BRS 10

*GET A LUD ENTRY
*EXPECTS ADDRESS OF FIRST WORD OF USER NAME IN POINT
*NO SKIP ERROR RETURN ERR CODE IN X 1=NAME NOT IN LUD
*SKIP NORMAL RETURN WITH EFFECTIVE ADDRESS OF ENTRY IN X

GENTRY ZRO; LDA POINT; MUL D3; LSH 23; SUB ONE; CAB; ADD D12; XAB
       STP GEPTR; CLA; LDX MFOUR; ADD* POINT; MIN POINT; BRX *-2
       LRSH 23; DIV =887; STB BLNO; CBA; SKG D895; BRU *+2; HLT
       BRM GE1
GE2    STX BLX; LDA 0,2; SKE OH; BRU *+3; LDX ONE; BRR GENTRY (NOT IN LUD)
       SKE MINONE; BRU *+3; MIN BLNO; BRU GE2-1; SKE MINTWO; BRU *+2
       BRU GE3; CXA; MUL D3; LSH 23; SUB ONE; CAB; ADD D12; XAB
       SKSE GEPTR; BRU GE3; LDX BLX; LDA 8,2; STA OCPARW
       MIN GENTRY; BRR GENTRY
GE3    LDX BLX; EAX 9,2; BRU GE2

*READ A FILE DIR GROUP INTO CORE

DIRR   ZRO; STA GRP; CLB; LRSH 23; DIV D64; SKG D3; BRU DIRR1
       SKG D35; BRU *+3; SUB D16; BRU DIRR1; SUB D4
       XAB; SUB O100; XAB
DIRR1  STB TLV3; CLB; LSH 12; ADD TLV3; LSH 1; ADD O200; CAB
       LDA =DBUF; LDX D128; STB DISC; BRS 124; BRR DIRR


*GETS NEXT AVAILABLE FILE DIRECTORY AND STORE
*NUMBER IN FDPT


*WRITE LAST FILE DIRECTORY GROUP READ

DIRW   ZRO; LDA =DBUF; LDB DISC; LDX D128; BRS 125; BRR DIRW

*RESET STRINGS



*SET DBUF TO ZERO

ZEROFD ZRO; LDX =-128; CLA; STA DBUFE,2; BRX *-1; BRR ZEROFD


*SETS F.D. FLAGS

SETFLG ZRO; LDA MINTWO; STA DBUF; LDA MINONE; STA DBUFE-1; BRR SETFLG

*OUTPUT OCTAL NUMBER TO TELETYPE

TONO   ZRO; LDB D8; LDX TOUT; BRS 36; BRR TONO
TDNO   ZRO; LDB D10; LDX TOUT; BRS 36; BRR TDNO
RONO   ZRO; LDX CIN; LDB D8; BRS 38; BRR RONO
RDNO   ZRO; LDX CIN; LDB D10; BRS 38; BRR RDNO

*MAKE AND SORT TABLE OF GANS

GANT   ZRO; SKR LISTSW; BRU *-1; BRM MFDT; BRM SORTL; CLA; STA LISTSW
       BRR GANT

GANT1  LDA 6,2 (GAN)
       ETR ETRACT; SKG OH; HLT; STA MTEMP; STX MTEMPX
       LDX =-600; LDA =LISBUF; STA LPTR
GANT2  LDA* LPTR; SKG OH; BRU GANT3; SKE MTEMP; BRU *+2; BRU GANT4
       MIN LPTR; BRX GANT2; HLT
GANT3  LDA MTEMP; STA* LPTR; LDA LPTR; STA ELPTR
GANT4  LDX MTEMPX; BRU MFDT3

SORTL  ZRO; MIN ELPTR; LDA =LISBUF
SORTL5 STA LPTR; ADD ONE; STA LPTR1
SORTL1 LDA LPTR1; SKE ELPTR; BRU *+2; BRU SORTL3
       LDA* LPTR1; SKG OH; BRR SORTL
       SKE* LPTR; BRU *+2; BRU *+3; SKG* LPTR; BRM EXCHL
       MIN LPTR1; BRU SORTL1
SORTL3 MIN LPTR; LDA LPTR; SKE ELPTR; BRU SORTL5; BRR SORTL
EXCHL  ZRO; XMA* LPTR; STA* LPTR1; BRR EXCHL
*POINTERS
PHP    DATA 3*UBUF+8
NAMPTA DATA 3*NAMBF1-1
FILPTR DATA 3*FILBUF-1,3*FILBUF-1
PTRFIL BSS 2
CIN    ZRO
TOUT   ZRO
CURMON ZRO
ASTER  DATA 12B
DIST   ZRO

START  STP CIN; SKG OH; STA CINSW
       BRS 71; BRU *+2; BRU ST1
       BRS 39; SKA =4B7; BRU *+2; BRU TYMERR
       RSH 2; ETR =377B; SKE TWO; BRU *+2; BRU ST1; SKE OH; BRU START3
ST1    BRM GUSER
       BRM RETFD; BRM DATE; LDA CRET; CIO TOUT; CIO TOUT
       BRU BEGIN
TYMERR MSG M4; BRS 10
START3 MSG ERR11; MSG ERR2; BRS 10

*COMMAND DISPATCHER

BEGIN  BRS 147
       SKN PSW1; BRU *+2; BRS 101
       SKN PSW1; BRU *+2; BRM GCREL
       LDA CIN; SKE OH
       BRU *+3
       LDA CRET; CIO TOUT; LDA ASTER; CIO TOUT
       LDX MINONE; BRS 13; BRM CLBUF
       CLA; LDX =TLV2-GRP; STA GRP,2; BRX *-1
       LDA =COMIN; STA CPTR
       BRM SGWD; BRM GWD; BRM GWD; BRM GWD
       CIO CIN; SKG 2SPA; BRU *-2
FCOM   STA TERCH
       LDA =COMDE; STA ENDCOM; LDX =COMD
FCOM1  LDA 0,2; ETR =@4B7; SKE COMIN; BRU FCOM2
       LDA 1,2; SKE COMIN+1; BRU FCOM2
       LDA 2,2; SKE COMIN+2; BRU FCOM2
       LDA 3,2; SKE COMIN+3; BRU FCOM2
       EXU 4,2
FCOM2  EAX 4,2; CXA; SKG ENDCOM; BRU FCOM3; LDA QUES; CIO TOUT; BRU BEGIN
FCOM3  EAX 1,2; BRU FCOM1
*GETS FD OF UN1
GUSER  ZRO; LDA D8; LDB =CALLER; LDX ONE; BRS 67; BRR GUSER
CALLER ZRO

*USING FILE ()LUD, FIRST CLEARS LUD DISC AREA AND REMAKES LUD

MGRERR MSG M3; CLA; STA VERSW; BRU BEGIN
SLUD   BRM VER; SKN MGRSW; BRU MGRERR
       BRS 113
       LDP LUDPT; BRS 64; BRU SLUDE; STA FNI; CLA
       LDX =-3777B; STA 24000B+3777B,2; BRX *-1 (CLEAR CORE PAGE)
       LDB =320200B-20000B; STB LOOPB; LDX =-7
SLUD1  STX LOOPX; LDA LOOPB; ADD =20000B; CAB; STB LOOPB
       BRM 4CPA; LDX LOOPX; BRX SLUD1 (CLEARS LUD AREA)
SLUD2  LDA =377B; CIT FNI; SKE ='   .'; BRU *+2; BRU SLUD5
       BRM SLUD2A; BRU SLUD3
SLUD2A ZRO; LDX MFOUR; CLA; STA NAMBF1+4,2; BRX *-1 (CLEAR NAME BUFFER)
       LDA NAMPTA; STA PTNBF1; STA PTNBF1+1; LDX =-12
       CIO FNI; WCI PTNBF1; BRX *-2; LDA PTNBF1+1; SUB PTNBF1
       SKE D12; HLT 0 (EVIDENT FILE FORMAT ERR)
       CLA; LDX MFOUR; ADD NAMBF1+4,2; BRX *-1
       LRSH 23; DIV =887; CBA; SKG D895; BRU *+2; HLT 0 (HASHING ERROR)
       STA BLNO; BRM GE1; STX BLX; BRR SLUD2A
SLUD3  LDA 0,2; SKE MINONE; BRU *+2; BRU NEWBLK; SKE OH; BRU SLUD4
       LDA NAMBF1; STA 0,2; LDA NAMBF1+1; STA 1,2; LDA NAMBF1+2
       STA 2,2; LDA NAMBF1+3; STA 3,2; BRM GONO; STA 4,2
       BRM GONO; STA 5,2; BRM GONO; STA 6,2; BRM GONO; STA 7,2
       BRM GONO; STA 8,2; CBA; SKE CRET; HLT 0 (FILE FORMAT ERROR)
       BRM WLUD; BRU SLUD2
SLUD4  EAX 9,2; CXA; SKE =UBOFL; BRU SLUD3; LDA MINONE; STA UBOFL
       BRM WLUD
NEWBLK MIN BLNO; BRM GE1; STX BLX; BRU SLUD3
SLUD5  CIO FNI; SKE CRET; BRU *-2
SLUD6  LDA =377B; CIT FNI; SKE ='   .'; BRU *+2; BRU SLUD7
       BRM SLUD2A; LDA =NAMBF1; STA POINT; BRM GENTRY
       BRU SLUD8 (COULD NOT FIND)
       LDA BLX; SUB UBUFAD; CLB; LSH 10; ADD BLNO; STA TLV5
       CIO FNI; SKE ='   ,'; HLT; BRM SLUD2A; LDA =NAMBF1; STA POINT
       BRM GENTRY; BRU SLUD9 (COULD NOT FIND); LDA 5,2
       ETR =@177777B
       MRG TLV5; STA 5,2; BRM WLUD; CIO FNI; SKE CRET; HLT; BRU SLUD6
SLUD7  LDA FNI; BRS 20; BRS 114; BRU BEGIN
SLUD8  CIO FNI; SKE ='   ,'; HLT; BRM SLUD2A
SLUD9  CIO FNI; SKE CRET; HLT; BRU SLUD6
4CPA   ZRO; LDA O24M; BRM CPA; BRM CPA; BRM CPA; BRM CPA; BRR 4CPA
CPA    ZRO; LDX O4000; BRS 125; XAB; ADD =40B; XAB; BRR CPA
WLUD   ZRO; LDA UBUFAD; LDX O100; LDB LDISC; BRS 125; BRR WLUD
GONO   ZRO; STX SAVX; LDX FNI; LDB D8; BRS 38; LDX SAVX; BRR GONO
LUDPT  DATA 3*LUDFIL-1,3*LUDFIL+4
LUDFIL ASC '()LUD'
LUDPT1 DATA 3*FILLUD-1,3*FILLUD-1+3
FILLUD ASC 'LUD'
SLUDE  MSG ERR10; BRS 10

*COMMAND TABLE

COMD   ASC 'HELP        '
       BRU HELP
       DATA 13024463B+4B7
       ASC 'T LUD    '
       BRU LLUD
       DATA 15231445B+4B7
       ASC 'RS       '
       BRU USERS
       DATA 11424454B+4B7
       ASC 'ES       '
       BRU FILES
       ASC 'CLEAR FILES '
       BRU CLFIL
       ASC 'FILE LICENSE'
       BRU SYSTEM
       ASC 'GROUPS      '
       BRU GROUPS
       DATA 11226445B+4B7
       ASC 'RGENCY ME'
       BRU SETEMG
       ASC 'CHECK SUPER '
       BRU CKSUP
       ASC 'SET LUD     '
       BRU SLUD
       ASC 'MAKE LUD FIL'
       BRU SVLUD
       DATA 13220451B+4B7
       ASC 'L BOX    '
       BRU POST
       DATA 14634463B+4B7
       ASC 'TEM PARAM'
       BRU SETPAR
       ASC 'UNUSED DIREC'
       BRU NUM
       DATA 14020443B+4B7
       ASC 'K RECORDS'
       BRU PACKS
       ASC 'VERSION     '
       BRU VERS
       ASC 'QUIT        '
       BRU THRU
       ASC 'Q           '
       BRU THRU
COMDE  EQU *

SGWD   ZRO; LDA SGWD; STA GWD; CLB
SGWD1  CIO CIN; SKG OH; BRU SGWD1; SKG 2SPA; BRU GWD+2; BRU SGWD1
GWD    ZRO; BRM GCH; LSH 16; STA* CPTR; BRM GCH; LSH 8
       ADM* CPTR; BRM GCH; ADM* CPTR; MIN CPTR; BRR GWD
GCH    ZRO; CIO CIN; SKG 2SPA; BRR GCH; BRU FCOM
CINERR MSG ERR2; BRS 10
*TYPES MAIL MSG NO., CREATION DATE & ORGINATOR ACCT NO./NAM NO.& COUNT

MLCNT  BRM     INIT
       BRM DATE
       LDX =-5000B; CLA; STA LETEN,2; BRX *-1
       LDA     MINONE
       STA     LETEN,2
       STA     TLV21
       LDA     =MBX
       LDB     =LMB
       LDX     O4000
       BRS     124            BRING IN MAIL LIST
       LDX     =-4000B
MLC1   LDA     EMBX,2
       SKE     OH
       BRU     *+2
       BRU     MLC1A
       SKN     EMBX,2          START OF LIST?
       BRU     MLC2
       SKE     MINONE
MLC1A  BRX     MLC1
       BRU     MLC3            END OF LIST
MLC2   XXA
       MIN     DTEMP,2         COUNT LETTERS
       XXA
       BRX     MLC1
MLC3   CLX
       STX     TLV1            CUR. MSG. NO.
       SKN     DTEMP,2         CK IF AT END OF LIST
       BRU     *+2
       BRU     MLC6
       LDA     DTEMP,2
       SKE     OH
       BRU     MLC5
MLC4   EAX     1,2
       BRU     MLC3+1
MLC5   STA     TLV12           SAVE COUNT, TYPE ALL INFO
       LDA TLV1; BRM GML; STX TLV11; LDA 0,2 (HAS LOCATER)
       BRM UNOUT
       LDA =MLC5A; STA JON; LDA TLV1
MLC5A  BRU JON4; BRM PONO (MSG NO.)
       SPACE 2; LDA =MLC5B; STA JON; LDA TLV12
MLC5B  BRU JON4; BRM PONO (COUNT)
       SPACE 3; LDX TLV11; LDA 1,2; BRM DATEA; LDX TLV1; BRU MLC4
MLC6   MSGP M17                MAIL LIST
       LDA =MBX; LDB =LMB; LDX O4000; BRS 124; LDX =-4000B
MLC7   STX     TLV1
       LDA EMBX,2; SKN EMBX,2; BRU MLC8; SKE MINONE; BRU *+2; BRU POST1
       ETR =37777777B; CAB; LDA CRET; CIO FNO; CBA; BRM UNOUT
       LDX     TLV1
MLC7A  BRX MLC7; HLT
MLC8   SKG OH; BRU MLC7A (IGNORE ZERO ENTRIES)
       LDB =8; LDX FNO; BRS 36 (TYPE MAIL NO.); LDX TLV1
*DECIDE WHETHER TO TYPE COMMA OR NOT
MLC8A  SKN EMBX+1,2; BRU *+2; BRU MLC7A (NEW LIST, DONT TYPE COMMA)
       LDA MINONE; SKA EMBX+1,2; BRU *+2 (NOT EMPTY, TYPE COMMA)
       BRX MLC8A (EMPTY); LDA ='   ,'; CIO FNO; BRU MLC7A

*GET A LETTER
GML    ZRO
       STA     TLV1
       ETR     =340B           GET PAGE NUMBER
       ADD     =LMBL
       SKE     TLV21           CK IF PAGE IS ALREADY IN
       BRU     GML2            NOT IN
       LDA     TLV1
       LSH     6
       ETR     =3700B          GET RELATIVE PAGE ADR.
       ADD     =MBX
       CAX
       BRR     GML
GML2   STA     TLV21           PUT PAGE NO IN PAGE SW.
       CAB
       LDA     =MBX
       LDX     O4000
       BRS     124
       LDA     TLV1            CUR MSG. NO.
       BRU     GML+1

*COPIES MAIL TO A FILES

COPML  BRM     INIT
       LDA     MINONE
       STA     TLV21
CPM1   MSG    EM2             ENTER MAIL NO.
       BRM    RONO
       SKG     =137B
       BRU     *+2
       BRU POST1
       BRM     GML
       ADD     TWO
       STA     TLV1            SAVE STARTING ADR.
       LDA     MINONE
       STA     TLV4            CONTROL CHAR. SW.
CPM2   LDA*    TLV1
       STA     TLV11
       BRM     TMXC
       BRM     TMXC
       BRM     TMXC
       BRM     TMXC
       MIN     TLV1            TO NEXT WORD
       BRU     CPM2

TMXC   ZRO     0               PUT A CHAR. ON FILE
       CLA
       LDB     TLV11           PICK UP WORD WITH REMAINING CHARS.
       LSH     6
       STB     TLV11
       SKN     TLV4            CK SW IF WORKING ON CONTROL CHAR.
       BRU     TMXC1           YES
       SKE     QUES            CK FOR CONTROL CHAR. SIGNAL
       BRU     *+3             NO
       STA     TLV4            SET SW
       BRR     TMXC
       CIO     FNO
       BRR     TMXC
TMXC1  LDB     MINONE
       STB     TLV4            RESTORE CONTROL CHAR. SW.
       SKE     QUES
       ADD     O100
       SKE     CDEE           CK IF END OF LETTER
       BRU     TMXC1-2         GO TYPE CHAR.
       BRU     CPM1

*CANCEL MAIL
CANML  MSG EM2 (ENTER MAIL NO.)
       BRS 113
       LDX     D7              LOC OF SWMBL IN MONITOR
       LDA ONE
       BRS     136
       SKE     MINONE             CK IF MAILBOX IS BUSY
       BRU     CNML1           YES
       LDA     =MBX
       LDB     =LMB
       LDX     O4000
       BRS     124
       BRM    RONO            GET MAIL NO.
       SKG     =137B
       SKG     MINONE
       BRU CANML1   ERROR
       CLB
       LDX     =-4000B
       SKE     EMBX,2
       BRU     *+2             NOT THIS LETTER
       STB     EMBX,2          FOUND LETTER
       BRX     *-3
       LDA     =MBX
       LDB     =LMB
       LDX     O4000          WRITE LIST BACK ON DISC
       BRS 125
       LDA MINONE; LDX D7; BRS 136
       BRS 114; BRU CANML
CANML1 LDA     MINONE
       LDX     D7              LOC OF SWMBL IN MONITOR
       BRS     136
       BRS 114; BRU POST1
CNML1  MSG    EM4             BUSY
       BRU POST1


*MAIL GARBAGE COLLECTION, REMOVES HOLES AND EMPTY LISTS
MLGBG  BRS 113; LDA ONE; STA FNO
       BRS 22
       LDX D7 (LOC OF SWMBL IN MON); LDA TWO; BRS 136; BRS 23; SKE MINONE
       BRU CNML1 (BUSY); LDA =MBX; STA TLV1; LDB =LMB; LDX O4000
       BRS 124; LDX =-4000B
MLG2   LDA EMBX,2 (SEARCH FOR A LIST); SKE MINONE (CK IF AT END OF LISTS)
       BRU MLG3 (NO)
MLG2A  STA* TLV1 (SET NEW END FLAG); LDA =MBX; STA TLV1; LDX =-4000B
MLG5   LDA EMBX,2; SKE MINONE; BRU MLG6; STA* TLV1; MIN TLV1
       LDA =MBX; LDB =LMB; LDX O4000; BRS 125 (WRITE OUT PACKED LISTS)
       BRS 22
       LDA MINONE; LDX D7; BRS 136 (RESTORE SW TO NOT BUSY); BRS 23
       MSG EM3 (TYPE LIST END AT:); LDA TLV1; SUB =MBX
       BRM PONO (TYPE LAST LOCATION); BRS 114; BRU POST1
MLG3   SKE OH; BRU MLG4
MLG3A  BRX MLG2; LDA MINONE; BRU MLG2A
MLG4   STA* TLV1; MIN TLV1; BRU MLG3A
MLG6   SKN EMBX,2; BRU MLG7; SKN EMBX+1,2 (MUST BE EMPTY LIST, IGNORE)
       BRU MLG7; BRX MLG5; HLT
MLG7   STA* TLV1; MIN TLV1; BRX MLG5; HLT
EM2    ASC '$ENTER MAIL NO.: /'
EM3    ASC '$MAIL LIST ENDS AT: /'
EM4    ASC '$BUSY.$/'
DATEA  ZRO; LDX =1200000B; BRS 176; STA MONTH; LRSH 20; STA YEAR
       CLA; LSH 4; ADD ONE; BRM PDNO; LDA SLASH; CIO FNO
       LDA MONTH; ETR =177777B; RSH 23; DIV =1440; STB MONTH
       ADD ONE; BRM PDNO; LDA SLASH; CIO FNO; LDA YEAR; ADD =72
       BRM PDNO; SPACE 2; LDA MONTH; RSH 23; DIV =60; STB MONTH
       BRM PDNO; LDA COLON; CIO FNO; LDA MONTH; SKG D9; BRU TDAT2
TDAT1  BRM PDNO; CR 1; BRR DATEA
TDAT2  LDA O20; CIO FNO; LDA MONTH; BRU TDAT1
MONTH  ZRO
YEAR   ZRO
M17    ASC '$MAIL LIST:'
       ASC '$USER NAME  -  MAIL NOS. -$/'
POS    ZRO
UNOUT  ZRO; STA BLNO; ETR =1777B; XMA BLNO; RSH 10; STA POS; BRM GE1
       CXA; ADD POS; LDB D12; LDX FNO; BRS 34; SPACE 2; BRR UNOUT

*COMMON ROUTINE FOR SUB COMMAND DISPATCHERS

SCOMD  ZRO
FRONT  CLA; STA ERFLG; LDX =-10; STA LISBUF+10,2
       BRX *-1; LDP CDPTR; STP WKPTR; LDA CRET; CIO TOUT; CIO TOUT
       LDA =36B; CIO TOUT
GTC    CIO CIN; SKE =141B; BRU *+3; BRM SCONTA; BRU GTC; SKG 2SPA
       BRU *+2; BRU GTC1; WCI WKPTR; BRU GTC
GTC1   LDA LISBUF; SKG OH; BRU FRONT; LDB =177777B; SKM OH
       BRU *+2; BRU GTC1A; LDB =377B; SKM OH; BRR SCOMD; BRU ERR
GTC1A  LRSH 16; SKE ='   Q'; BRU *+2; BRU THRU; SKE QUES; BRU ERR
GTC1B  NOP 0 (CALLING ROUTINE MUST STORE VECTOR HERE)
ERR    LDA ERFLG; SKE OH; BRU ERR1A; MIN ERFLG
       LDA QUES; CIO TOUT; LDA CRET; CIO TOUT
       CLAB; BRU FRONT+2
ERR1A  MSG ERMSG1; BRU FRONT
ERMSG1 ASC '$FOR ASSISTANCE TYPE HELP$/'
CDPTR  DATA 3*LISBUF-1,3*LISBUF-1
WKPTR  BSS 2
ERFLG  ZRO
SCONTA ZRO; LDA 2SPA; CIO TOUT; GCD WKPTR; NOP; BRR SCONTA

*ROUTINE TO SET SYSTEM CONTROL PARAMETERS

SETPAR BRM RCBLK; CLA; STA WSW (CLEAR DISC WRITE SWITCH)
       LDA SPBRU; STA GTC1B
SP1    BRM SCOMD; LDX =SPCBL-SPCBLD; SKE SPCBLD,2; BRX *-1
       BRU* SPBBLD,2
SPCBL  ASC 'HELDISZONHOSSYSEQUQUIRET'
SPCBLD DATA SPHEL,SPDIS,SPZON,HOST,SYS,EQ,THRU,BEGIN
SPBBLD DATA ERR

WSW    ZRO
SETWR  ZRO; SKR WSW; BRU *-1; BRR SETWR
SPBRU  BRU SPHEL

SPHEL  MSG M1; MSG SPHELM; MSG CHMSG; MSG CHMSG1; MSG SHORTM; BRU SP1
SPHELM ASC '$$DISPLAY         DISPLAYS CURRENT STATUS OF FOLLOWING PARAMETERS'
       ASC '$EQUIPMENT       *SETS SYSTEM EQUIPMENT AND SYSTEM USE PARAMETERS'
       ASC '$HOST            *SETS SYSTEM HOST NUMBER FOR DIM AND TYMNET'
       ASC '$SYSTEM          *SETS SYSTEM NAME'
       ASC '$ZONES           *SETS NUMBER OF MAP ZONES/'
CHMSG  ASC '$RETURN          RETURNS TO PRIME COMMAND DISPATCHER'
       ASC '$Q OR QUIT       RETURNS TO THE EXEC'
       ASC '$HELP OR ?       RETYPES THIS LIST/'
CHMSG1 ASC '$$* INDICATES MANAGER LICENSE IS REQUIRED./'
SHORTM ASC '$$ALL OF THE ABOVE COMMANDS MAY BE SHORTENED TO THEIR FIRST'
       ASC '$THREE CHARACTERS$/'

*DISPLAY SYSTEM PARAMETERS

SPDIS  MSG SPDM1; BRM PSYSN; BRM PHNUM; BRM PEQU; BRM TMZONE; BRU SP1
PEQU   ZRO; MSG SPDM4; LDA SYSPAR
       SKA TWO; MSG SPDM6; SKA FOUR; MSG SPDM7
       SKA O20; MSG SPDM8; SKA =40B; MSG SPDM9
       SKA O200; MSG SPDM10; SKA =400B; MSG SPDM11
       LRSH 9; ETR =7; SKE =1; BRU *+3; MSG SPDM12; BRR PEQU
       SKE =3; BRU *+3; MSG SPDM13; BRR PEQU
       SKE =4; BRU *+4; MSG SPDM14; BRR PEQU
       MSG SPDM15; BRR PEQU
TMZONE ZRO; MSG SPDM16; LDA SYSMAP; CNA; ADD ONE
       BRM TDNO; BRR TMZONE
SPDM1  ASC '$CURRENT PARAMETERS ARE: /'
SPDM2  ASC '$SYSTEM NAME: /'
SPDM3  ASC '$HOST NO: /'
SPDM4  ASC '$EQUIPMENT AND SOFTWARE: /'
SPDM6  ASC '$NON-STANDARD SOFTWARE/'
SPDM7  ASC '$PRINTER/'
SPDM8  ASC '$TAPE DRIVE 0/'
SPDM9  ASC '$TAPE DRIVE 1/'
SPDM10 ASC '$CARD PUNCH/'
SPDM11 ASC '$CARD READER/'
SPDM12 ASC '$DATA PRODUCTS DISK/'
SPDM13 ASC '$SINGLE DENSITY PACKS/'
SPDM14 ASC '$DUAL DENSITY PACKS/'
SPDM15 ASC '$DISC CONTROL NOT SET/'
SPDM16 ASC '$MAP ZONES: /'
SPDM17 ASC '$NO. OF MAP ZONES? /'
SPDM18 ASC '$OKAY? /'
SPDM19 ASC '$ENTER NEW HOST NO.: /'
SPDM20 ASC '$ENTER NEW SYSTEM NAME: /'

SPRET  SKN WSW; BRU SP1
       LDA SYSPAR; LDB SYSMAP; LDX SYSNAM; STP MSAVA; STX MSAVX
       BRM RCBLK; LDP MSAVA; LDX MSAVX; STA SYSPAR; STB SYSMAP; STX SYSNAM
       BRM WCBLK; CLA; STA WSW; BRU SP1

*SET NUMBER OF MAP ZONES

SPZON  BRM VER; SKN MGRSW; BRU MGRERR
SPZ1   MSG SPDM17; BRM CLBUF; BRM RDNO; SKG =8; SKG TWO; BRU SPZ1
       STA MSAVA; LRSH 23; DIV TWO; CBA; SKE OH; BRU SPZ1
       LDA MSAVA; SUB ONE; CNA; STA SYSMAP; BRM TMZONE; BRM OKAY
       BRU SPZ1; BRM SETWR; BRU SPRET

RCBLK  ZRO; LDA =SYSBUF; LDB O200; LDX O100; BRS 124; BRR RCBLK
WCBLK  ZRO; LDA =SYSBUF; LDB O200; LDX O100; BRS 125; BRR WCBLK
PSYSN  ZRO; MSG SPDM2; LDA =SYSNAM; LDB THREE; LDX TOUT; BRS 34; BRR PSYSN
PHNUM  ZRO; MSG SPDM3; LDA SYSPAR; LRSH 12; BRM TDNO; BRR PHNUM

*SET HOST NUMBER

HOST   BRM VER; SKN MGRSW; BRU MGRERR
HOST1  MSG SPDM19; BRM CLBUF; BRM RDNO; SKG =127; SKG OH; BRU HOST1
       CLB; LSH 12; STA MSAVA; LDA SYSPAR; ETR =7777B; MRG MSAVA
       STA SYSPAR; BRM PHNUM; BRM OKAY; BRU HOST1; BRM SETWR; BRU SPRET

*SET SYSTEM NAME

SYS    BRM VER; SKN MGRSW; BRU MGRERR
SYS1   CLA; STA LISBUF; LDP CDPTR; STP WKPTR; MSG SPDM20
       BRM CLBUF
SYS2   CIO CIN; SKE =141B; BRU *+3; BRM SCONTA; BRU SYS2; SKG 2SPA
       BRU *+2; BRU SYS3; WCI WKPTR; BRU SYS2
SYS3   LDA LISBUF; STA SYSNAM; BRM PSYSN
       BRM OKAY; BRU SYS1; BRM SETWR; BRU SPRET
OKAY   ZRO; MSG SPDM18; BRM CLBUF; BRM YN; BRR OKAY; MIN OKAY; BRR OKAY

*SET EQUIPMENT PARAMETERS

EQ     BRM VER; SKN MGRSW; BRU MGRERR
EQ1    LDA SYSPAR; MRG ONE (ALWAYS SET TYMNET BIT)
       MSG SPDM6; MSG MSGQUE
       BRM CLBUF; BRM YN; BRU *+3; MRG TWO; BRU *+2
       ETR =@2; MSG SPDM7; MSG MSGQUE; BRM CLBUF; BRM YN; BRU *+3
       MRG FOUR; BRU *+2; ETR =@4; MSG SPDM8; MSG MSGQUE
       BRM CLBUF; BRM YN; BRU *+3; MRG O20; BRU *+2; ETR =@20B
       MSG SPDM9; MSG MSGQUE; BRM CLBUF; BRM YN; BRU *+3; MRG =40B
       BRU *+2; ETR =@40B; MSG SPDM10; MSG MSGQUE; BRM CLBUF
       BRM YN; BRU *+3; MRG O200; BRU *+2; ETR =@200B
       MSG SPDM11; MSG MSGQUE; BRM CLBUF; BRM YN; BRU *+3; MRG =400B
       BRU *+2; ETR =@400B; ETR =@7B3
EQ2    MSG SPDM14; MSG MSGQUE; BRM CLBUF; BRM YN; BRU *+3; MRG =4B3
       BRU EQEND; MSG SPDM12; MSG MSGQUE; BRM CLBUF; BRM YN
       BRU *+3; MRG =1B3; BRU EQEND; MSG SPDM13; MSG MSGQUE
       BRM CLBUF; BRM YN; BRU EQ2; MRG =3B3
EQEND  STA SYSPAR; BRM PEQU; BRM OKAY; BRU EQ1; BRM SETWR; BRU SPRET
MSGQUE ASC '? /'

SETEMG LDA SEBRU; STA GTC1B
SE1    BRM SCOMD; LDX =SECBL-SECBLD; SKE SECBLD,2; BRX *-1
       BRU* SEBBLD,2
SECBL  ASC 'STASETCLEHELQUIRET'
SECBLD DATA STAT,SET,CLEAR,SEHEL,THRU,BEGIN
SEBBLD DATA ERR
SEBRU  BRU SEHEL
SEHEL  MSG M1; MSG SEHELM; MSG CHMSG; MSG SHORTM; BRU SE1
SEM1   ASC '$EMERGENCY MESSAGE FLAG NOT SET$/'
SEM2   ASC '$EMERGENCY MESSAGE FLAG SET$/'
SEHELM ASC '$$STATUS          REPORTS THE CURRENT STATUS OF THE'
       ASC '$                EMERGENCY MESSAGE SWITCH'
       ASC '$SET             SETS EMERGENCY MESSAGE SWITCH TO INDICATE'
       ASC '$                AN EMERGENCY MESSAGES IS WAITING.'
       ASC '$CLEAR           RESETS THE EMERGENCY MESSAGE SWITCH/'
SEM5   ASC '$EMERGENCY MESSAGE SWITCH TURNED ON$/'
SEM6   ASC '$EMERGENCY MESSAGE SWITCH TURNED OFF$/'

*READ AND REPORT CURRENT SETTING OF EMERGENCY MESSAGE SWITCH

STAT   BRM REFLG; LDA FLAG; SKA =1; BRU STAT1; MSG SEM1 (NOT SET)
       BRU SE1
STAT1  MSG SEM2 (SET); BRU SE1

*SET EMERGENCY MESSAGE SWITCH ON  

SET    BRM VER; BRM SEFLG; MSG SEM5 (TURNED ON); BRU SE1

*SET EMERGENCY MESSAGE SWITCH OFF   

CLEAR  BRM VER; BRM CEFLG; MSG SEM6 (TURNED OFF); BRU SE1

REFLG  ZRO; BRS 22; LDX FOUR; CLA; BRS 136; STA FLAG; BRS 136; BRS 23
       BRR REFLG

SEFLG  ZRO; BRS 22; CLA; LDX FOUR; BRS 136; MRG =1; BRS 136; BRS 23
       BRR SEFLG

CEFLG  ZRO; BRS 22; CLA; LDX FOUR; BRS 136; ETR =@1; BRS 136
       BRS 23; BRR CEFLG
FLAG   ZRO

*PRINT LISTING OF SYSTEM OR ABOVE PROGRAMS

SYSTEM BRM INIT; BRM CMFDT; SKR LICSW; BRU *-1; CLA; STA FDPT
SSYS2   MIN FDPT; LDA FDPT; SKG BUFBAS; BRU *+3; MIN LICSW; BRU BEGIN
       BRM PGZERO; BRU SSYS2
       BRM DIRR; LDA DBUF; SKE OH; BRU *+2; BRU SSYS2
       LDX =DBUF
SSYS3   LDA 0,2; SKG OH; BRU SSYS5
       STX TLV23; LDA 2,2; SKA D6; BRU SSYS4
       EAX 4,2
SSYS3A  EAX 1,2; SKN -1,2; BRU *-2; BRU SSYS3
SSYS4   STX TLV23; BRM PFLIN1; BRM SSYS6; BRU SSYS3A
SSYS5   SKE MINTWO; BRU *+2; BRU SSYS2; LDX =DBUF; LDA 124,2
       SKG OH; BRU SSYS2
       LDA 125,2; SKG OH; BRU SSYS2; BRM DIRR; LDX =DBUF; BRU SSYS3
SSYS6  ZRO; LDX TLV23; LDA 2,2; ETR D6; SKE D6; BRU *+3
       MSGP SSM1; BRU *+2; MSGP SSM2; MIN TLV23; MIN TLV23; MIN TLV23
       MIN TLV23; LDA =SSYS7; STA PDIR
SSYS7  BRU PDIR+6; BRR SSYS6
SSM1   ASC ' EXEC    /'
SSM2   ASC ' SYSTEM  /'
LICSW  ZRO

*MAIL BOX ROUTINES

POST   LDA POSBRU; STA GTC1B
POST1  BRM SCOMD; LDX =PCBL-PCBLD; SKE PCBLD,2; BRX *-1
       BRU* PBBLD,2
PCBL   ASC 'COPLISCLECANHELQUIRET'
PCBLD  DATA COPML,MLCNT,MLGBG,CANML,PHEL,THRU,BEGIN
PBBLD  DATA ERR
POSBRU BRU PHEL
PHEL   MSG M1; MSG PHELM; MSG CHMSG; MSG CHMSG1; MSG SHORTM; BRU POST1
PHELM  ASC '$$COPY LETTER     OUTPUTS A LETTER TO TELETYPE'
       ASC '$LIST MAIL BOX   LISTS ACTIVE MAIL AND ADDRESSEES'
       ASC '$CLEAR GARBAGE   GARBAGE COLLECTS THE MAIL BOX'
       ASC '$CANCEL LETTER   CANCELS A LETTER/'

SVLUD  BRS 113; LDA =1; CAX; BRS 141; LDP LUDPT1; LDX THREE; BRS 65
       HLT 0; STA FNO; SKR SLUDSW; BRU *-1; BRM GOPER; BRM MFDT
       LDX MFOUR; LDA PERIOD; CIO FNO; BRX *-1
       LDA CRET; CIO FNO; CLA; STA SLUDSW; BRM PROJP
       LDA FNO; BRS 147; CLA; STA SPROSW; BRM RETFD; BRS 114; BRU BEGIN
SVLUD1 STX BX; BRM SKOPER; CXA; LDB D12; LDX FNO; BRS 34; LDX BX
       LDA 4,2; BRM PONO; LDA =14B; CIO FNO; LDA 5,2; BRM PONO
       LDA =14B; CIO FNO; LDA 6,2; BRM PONO; LDA =14B; CIO FNO
       LDA 7,2; BRM PONO; LDA =14B; CIO FNO; LDA 8,2; BRM PONO
       LDA CRET; CIO FNO; SKN OPERSW; BRU *+2; BRU GOPER1
       BRU MFDT3
PROJP  ZRO; SKR SPROSW; BRU *-1; BRM MFDT; LDX MFOUR
       LDA PERIOD; CIO FNO; BRX *-1; LDA CRET; CIO FNO; BRR PROJP
PROJP1 STX BX; LDA 5,2; ETR =177777B; SKG OH; BRU MFDT3
       LRSH 10; STA TLV4; CLA; LSH 10; XMA BLNO; STA TLV5
       BRM GE1; LDA TLV5; STA BLNO; LDA TLV4; ADD =UBUF; CAX; LDA 0,2
       SKG OH
       BRU PROJP2; CXA; LDB D12; LDX FNO; BRS 34
       LDA =14B; CIO FNO; LDA BX; LDB D12
       LDX FNO; BRS 34; LDA CRET; CIO FNO
PROJP2 LDX BX; BRU MFDT3
SPROSW ZRO
SLUDSW ZRO
BX     ZRO
OPSTG  ASC 'OPER        '
OPERPT DATA 3*OPSTG-1,3*OPSTG-1+12

GOPER  ZRO; LDA =OPSTG; STA POINT; BRM GENTRY; HLT 0 (NO OPER)
       STX BX; SKR OPERSW; BRU *-1; BRU SVLUD1+2
GOPER1 MIN OPERSW; BRR GOPER
OPERSW ZRO

SKOPER ZRO; CXA; MUL D3; LSH 23; SUB ONE; CAB; ADD D12; XAB; SKSE OPERPT
       BRR SKOPER; BRU MFDT3


*BYPASS PAGE ZERO FILE DIRECTORIES

PGZERO ZRO; SKG =417B; SKG =377B; MIN PGZERO; BRR PGZERO

*MAKE LIST OF UNUSED FILE DIRECTORIES

NUM    BRM CMFDT; BRM INIT; BRM DATE; MSG NUMM1
       LDX =FDTAB+1
NUM1   LDA CRET; CIO FNO; CXA; SKE =33777B; BRU *+2; BRU NUM6
       SKN 0,2; BRU *+2; BRU NUM4; LDA 0,2; SKE OH; BRU NUM4; CXA; SUB =30000B
       BRM PONO; BRM FIXX; SKN 1,2; BRU *+2; BRU NUM5
       LDA 1,2; SKE OH; BRU NUM5
       LDA DASH; CIO FNO
NUM2   EAX 1,2; CXA; SKE =33777B; BRU *+2; BRU NUM6C
       SKN 1,2; BRU *+2; BRU NUM3; LDA 1,2; SKG OH; BRU NUM2
NUM3   CXA; SUB =30000B; BRM PONO; BRM FIXX
NUM5   EAX 1,2; BRU NUM1
NUM4   EAX 1,2; BRU NUM1+2
NUM6   LDA 0,2; SKE OH; BRU NUM6A
NUM6B  CXA; ETR =37777B; SUB =30000B; BRM PONO
NUM6A  LDA CRET; CIO FNO; BRU BEGIN
NUM6C  LDA 0,2; SKE OH; BRU *+2; BRU NUM6B; CXA; ETR =37777B
       SUB ONE; BRU NUM6B+3
FIXX   ZRO; CXA; ETR =37777B; CAX; BRR FIXX
NUMM1  ASC '$UNASSIGNED FILE DIRECTORY NUMBERS$/'
PACKS  BRM RCBLK; LDA SYSPAR; LRSH 9; ETR =7; SKG =1; BRU PKERR
       LDA SYSMAP; CNA; STA NPACKS; LDA PACBRU
       STA GTC1B
PKR1   BRM SCOMD; LDX =PKCBL-PKCBLD; SKE PKCBLD,2; BRX *-1
       BRU* PKBBLD,2
PKCBL  ASC 'HELREASYSPACMAPSERRETSETQUI'
PKCBLD DATA PKHEL,RPAK,CSYSNO,PAKNO,PKMAP,SERIAL,BEGIN,PKSET,THRU
PKBBLD DATA ERR
PACBRU BRU PKHEL
NPACKS ZRO
PKERR  MSG PKERRM; BRU BEGIN
PKERRM ASC '$NOT A DISC PACK SYSTEM$/'

PKHEL  MSG M1; MSG PKHELM; MSG CHMSG; MSG CHMSG1; MSG SHORTM; BRU PKR1
PKHELM ASC '$$READ RECORD     READS CURRENT RECORD OF A PACK'
       ASC '$SYSTEM          *CHANGES THE SYSTEM NO. ON A PACK'
       ASC '$PACK NO.        *CHANGES THE PACK NO. OF A PACK'
       ASC '$SET NO.         *CHANGE THE SET NUMBER OF A PACK'
       ASC '$MAP TIME        *CHANGES LAST TIME MAPPED ON A PACK'
       ASC '$SERIAL NO.      *CHANGES SERIAL NO. OF A PACK/'
RPAK   BRM GPAKNO; BRM RDPAK; MSG KM1; LDA PNUMB; BRM TDNO
       MSG KM2; LDA =PSYS; LDB THREE; LDX TOUT; BRS 34
       MSG KM3; LDA PSET; BRM TDNO
       MSG KM4; LDA PMAP; BRM TTIME; MSG KM7; LDA PCOP
       BRM TTIME; MSG KM7A; LDA PSER; BRM TDNO
       LDA CRET; CIO TOUT; BRU PKR1
CSYSNO BRM VER; SKN MGRSW; BRU MGRERR; BRM GPAKNO; BRM RDPAK
       MSG KM12; LDA =PKBUF; LDB THREE; LDX TOUT; BRS 34
       MSG KM13; LDP HSYSP; STP SYSP; CLA; STA CHCNT; LDX =-3
       CIO CIN; SKE CRET; BRU *+2; BRU CSYSN2; WCI SYSP; MIN CHCNT
       BRX *-6; CIO CIN; SKE CRET; BRU *-2
CSYSN2 LDA CHCNT; SKE THREE; BRU CSYSN4; LDA SYSN
CSYSN3 STA SYSN; MSG KM12
       LDA =SYSN; LDB THREE; LDX ONE; BRS 34; BRM OKAY; BRU CSYSNO+5
       LDA SYSN; STA PKBUF; STA PKBUF1; BRM WPAK; BRU PKR1
CSYSN4 SKG ONE; BRU CSYSN5; LDA SYSN; LRSH 8; BRU CSYSN3
CSYSN5 LDA SYSN; LRSH 16; BRU CSYSN3
CHCNT  ZRO
SYSN    ZRO
HSYSP  DATA 3*SYSN-1,3*SYSN-1
SYSP   BSS 2
TTIME  ZRO; STA TEMP; CLB; RSH 16; ADD ONE; BRM TDNO; LDA SLASH; CIO TOUT
       LDA TEMP; ETR =177777B; RSH 23; DIV =1440; STB TEMP
       ADD ONE; BRM TDNO; LDA OH; CIO TOUT; LDA TEMP; RSH 23
       DIV =60; STB TEMP; BRM TDNO; LDA COLON; CIO TOUT; LDA TEMP
       SKG D9; BRM PKZERO; BRM TDNO; BRR TTIME
PKBUF  EQU 24000B
PKBUF1 EQU 24400B
PNUMB  EQU PKBUF+1
PSYS   EQU PKBUF
PSET   EQU PKBUF+2
PMAP   EQU PKBUF+3
PCOP   EQU PKBUF+4
PSER   EQU PKBUF+5
PNUMB1 EQU PKBUF1+1
PSYS1  EQU PKBUF1
PSET1  EQU PKBUF1+2
PMAP1  EQU PKBUF1+3
PCOP1  EQU PKBUF1+4
PSER1  EQU PKBUF1+5

RDPAK  ZRO; STA TEMP; MUL =128000; LSH 23; ADD =64000; CAB; STB DISC
       LDA =PKBUF; LDX =400B; BRS 124
       BRR RDPAK
WPAK   ZRO; LDB DISC; LDA =PKBUF; LDX =400B; BRS 125; BRR WPAK
GPAKNO ZRO; MSG KM1; BRM CLBUF; BRM RDNO; STA PACKNO; SKG NPACKS
       SKG MINONE; BRU GPAKNO+1; LRSH 23; DIV TWO; CBA; SKE OH
       BRU GPAKNO+1; LDA PACKNO; BRR GPAKNO
KM1    ASC '$PACK NO: /'
KM2    ASC '$SYSTEM /'
KM3    ASC '$SET NO. /'
KM4    ASC '$LAST MAPPED /'
KM7    ASC '$LAST COPIED /'
KM7A   ASC '$SERIAL NO. /'
KM10   ASC '$PRESENT PACK NO. IS /'
KM11   ASC '$ENTER NEW PACK NO. /'
KM12   ASC '$PRESENT SYSTEM NO. IS /'
KM13   ASC '$ENTER NEW SYSTEM NO. /'
KM14   ASC '$PRESENT SET NO. IS /'
KM15   ASC '$ENTER NEW SET NO. /'
KM16   ASC '$ENTER CORRECTED MAPPED TIME AS:'
       ASC '$MONTH-DAY TIME /'
KM18   ASC '$PRESENT SERIAL NO. IS /'
KM19   ASC '$ENTER NEW SERIAL NO. /'
PACKNO ZRO
PAKNO  BRM VER; SKN MGRSW; BRU MGRERR; BRM GPAKNO; BRM RDPAK
       MSG KM10; LDA PNUMB; BRM TDNO; LDA =PAKNO1; STA GPAKNO; MSG KM11
PAKNO1 BRU GPAKNO+2; STA PNUMB; ADD ONE; STA PNUMB1; MSG KM10
       LDA PNUMB; BRM TDNO; BRM OKAY; BRU PAKNO1-1; BRM WPAK; BRU PKR1
SERIAL BRM VER; SKN MGRSW; BRU MGRERR; BRM GPAKNO; BRM RDPAK
       MSG KM18; LDA PSER; BRM TDNO
SER1   MSG KM19; BRM CLBUF; BRM RDNO; STA PSER; STA PSER1
       MSG KM18; LDA PSER; BRM TDNO; BRM OKAY; BRU SER1; BRM WPAK; BRU PKR1
PKSET  BRM VER; SKN MGRSW; BRU MGRERR; BRM GPAKNO; BRM RDPAK; MSG KM14
       LDA PSET; BRM TDNO
PKSET1 MSG KM15; BRM CLBUF; BRM RDNO; STA PSET; STA PSET1
       MSG KM14; LDA PSET; BRM TDNO; BRM OKAY; BRU PKSET1; BRM WPAK; BRU PKR1
PKZERO ZRO; STA SAVA; LDA O20; CIO TOUT; LDA SAVA; BRR PKZERO
PKMAP  BRM VER; SKN MGRSW; BRU MGRERR
       BRM GPAKNO; BRM RDPAK
       MSG KM4; LDA PMAP; BRM TTIME
PKMAP1 MSG KM16
       BRM RDNO; CLB; SUB ONE; LSH 16; STA TEMP; BRM RDNO; SUB ONE
       CLB; MUL =720; CBA; ADM TEMP; BRM RDNO; LRSH 23; DIV =100
       STB MINS; MUL =60; LSH 23; ADD MINS; ADM TEMP; LDA TEMP
       STA PMAP; STA PMAP1; MSG KM4; LDA PMAP; BRM TTIME
       BRM OKAY; BRU PKMAP1; BRM WPAK; BRU PKR1
MINS   ZRO

*LIST ALL USERS ON SYSTEM

USERS  BRM RETFD; LDA UBRU; STA GTC1B; LDA UBRU1; STA LLUD8A
U1     CLA; STA BLNO
       BRM SCOMD; LDX =UCBL-UCBLD; SKE UCBLD,2; BRX *-1; BRU* UBBLD,2
UCBL   ASC 'GANUUNNAMFILHELRETQUI'
UCBLD  DATA UG,UU,UNA,UFDL,UHEL,BEGIN,THRU
UBBLD  DATA ERR
UBRU   BRU UHEL
UBRU1  BRU U1
UHEL   MSG UHELM; MSG CHMSG; MSG SHORTM; BRU U1
UHELM  ASC '$$OUTPUTS A LIST OF ALL USERS TO TELETYPE, FILE, OR PRINTER'
       ASC '$SORTED IN ACCORDANCE WITH ONE OF THE FOLLOWING COMMANDS:'
       ASC '$$GAN LIST        SORTED BY GLOBAL ACCOUNT NO.'
       ASC '$UUN LIST        SORTED BY UNIVERSAL USER NO.'
       ASC '$NAME LIST       SORTED BY USER NAME'
       ASC '$FILE DIR. LIST  SORTED BY FILE DIRECTORY NO./'
UG     LDA O41; BRU USERS1
UU     LDA O65; BRU USERS1
UNA    LDA O56; BRU USERS1
UFDL   LDA O46; BRU USERS1

*LIST FILES

FILES  BRM RETFD; LDA FBRU; STA GTC1B; LDA FBRU1; STA LLUD8
F1     CLA; STA FDPT; STA BLNO
       BRM SCOMD; LDX =FCBL-FCBLD; SKE FCBLD,2; BRX *-1; BRU* FBBLD,2
FCBL   ASC 'ALLFILUSEHELRETQUI'
FCBLD  DATA AF,FF,UF,FHEL,BEGIN,THRU
FBBLD  DATA ERR
FBRU   BRU FHEL
FBRU1  BRU F1
FHEL   MSG M1; MSG FHELM; MSG CHMSG; MSG SHORTM; BRU F1
FHELM  ASC '$$ALL             LISTING OF ALL FILES'
       ASC '$FILE DIR.       LISTING OF FILES FOR A SPECIFIED F.D. NO.'
       ASC '$USER            LISTING OF FILES FOR A SPECIFIED USER NAME/'
AF     BRM FINIT; CR 1; SKR FSFSW; BRU *-1; BRU FILES1
FF     BRM FINIT; CR 1; SKR FSFSW; BRU *-1; BRU FSF1
UF     BRM FINIT; CR 1; BRU NSFILE
FINIT  ZRO; BRM INIT; SKN FNO; BRR FINIT; BRM GPREL
       SKR PSW1; BRU *-1; BRR FINIT

*LIST THE LUD

LLUD   CLA; STA IMLSW
       BRM RETFD; LDA LLBRU; STA GTC1B; LDA LLBRU1; STA LLUD8A
LL1    CLA; STA BLNO; BRM SCOMD; LDX =LLCBL-LLBLD; SKE LLBLD,2; BRX *-1
       BRU* LLBBLD,2
LLCBL  ASC 'HELALLGANIMLRESQUIRET'
LLBLD  DATA LLHEL,LLALL,LLGAN,LIML,LRESL,THRU,BEGIN
LLBBLD DATA ERR
LLBRU  BRU LLHEL
LLBRU1 BRU LL1
LLHEL  MSG M1; MSG LLHELM; MSG CHMSG; MSG SHORTM; BRU LL1
LLHELM ASC '$$ALL             LIST COMPLETE LUD'
       ASC '$GAN             LIST SPECIFIED GLOBAL ACCOUNT'
       ASC '$IML LIST        LIST COMPLETE LUD IN FORMAT USABLE BY IML'
       ASC '$RESTRICTED      LIST OF USERS AND GANS RESTRICTED ON THIS SYS-'
       ASC '$                TEM, IML COMPATABLE./'
LLALL  BRM FINIT; BRU LLUD1A
LLGAN  BRM FINIT; BRU LLUD5
LIML   BRM FINIT; SKR IMLSW; BRU *-1; BRU LLUD1B
LRESL BRM FINIT; LDA MINONE; STA IMLSW; STA RESTSW; BRU LLUD1B

*CHECK PROGRAM SUPER IN SUP TO SEE IF HAS EXEC LICENSE

CKSUP  LDA =3017B; LDX =1; BRS 141 (GFD SUP)
       LDP SUPER; BRS 48; BRU CKSUP1; LDB =SUPBUF; LDX =10
       BRS 96; LDA SUPBUF+3; ETR =6; SKE =6; BRU *+3
       MSG SM1 (HAS LICENSE); BRU BEGIN
       MSG SM2 (NO LICENSE); BRU BEGIN
CKSUP1 MSG SM3 (NO PROGRAM SUPER); BRU BEGIN
SUPN   ASC 'SUPER'
SUPER  DATA 3*SUPN-1,3*SUPN-1+5
SUPBUF BSS 20
SM1    ASC '$SUPER LICENSE OKAY$/'
SM2    ASC '$SUPER LICENSE BAD - NOTIFY SYSTEMS$/'
SM3    ASC '$PROGRAM SUPER NOT FOUND - IF THIS IS A SUPERVISOR'
       ASC '$SYSTEM NOTIFY SYSTEMS$/'
       END
