        MOVEM   R1,T3
        MOVNI   R3,5
        AOS     T3
        MOVEI   R1,3
        HRL     R1,AUX
        AUXCAL  R1,T3
        AOJL    R3,.-4
ICM1:   MOVE    R1,T2#
        MOVEM   R1,T1#
        PUSHJ   P,GCHA
        MOVE    CH,R1
        MOVEM   CH,T2
        SUB     CH,T1
        CAIE    CH,1
        JRST    ICM1
        MOVE    R1,T2
ICM2:   MOVEM   R1,T1
ICM3:   PUSHJ   P,GCHA
        MOVE    CH,R1
        MOVEM   CH,T2
        SUB     CH,T1
        CAIE    CH,1
        JRST    ICM1
        MOVE    R1,T2
        CAME    R1,T3#
        JRST    ICM2
;
;       SEND COMMAND WORD
;
        MOVE    R1,CMD#
SCMD:   PUSHJ   P,SWD
        PUSHJ   P,GWDA
        CAME    R1,CMD
        ERROR   SYSER
        PUSHJ   P,CLTIM
        POPJ    P,

SWD:	MOVEI   R2,3
        HRL     R2,AUX
        SETZ    R0,
        LSHC    R0,24
        AUXCAL  R2,R0

        SETZ    R0,
        LSHC    R0,10
        AUXCAL  R2,R0

        SETZ    R0,
        LSHC    R0,10
        AUXCAL  R2,R0

        POPJ    P,

GWDA:	HRLZ    R2,AUX
        SETZ    R0,
        PUSHJ   P,GCHA
        OR      R0,R1
        LSH     R0,10
        PUSHJ   P,GCHA
        OR      R0,R1
        LSH     R0,10
        PUSHJ   P,GCHA
        OR      R1,R0
        POPJ    P,

GCHA:   MOVEI   R2,10
        HRL     R2,AUX
        HRRI    R2,2
        AUXCAL  R2,R1
        ERROR   SYSER
        POPJ    P,
GCHA1:  MOVEI   R2,1000
        SLEEP   R2,
        JRST    GCHA
