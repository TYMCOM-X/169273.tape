1OPCK ** FICHE/FRAME BREAK *****
CKOP3
OPCK   IDENT   11-6-74

       BRU START; BRS 10

VER    ASC 'OPCK VERSION 3 /'; ASC 'BLOOD'

MSG    POPD 100B5,2
       EAX* 0; CXA; ETR =37777B; LDB =-1; LDX =1; BRS 34; BRR 0

START  MSG VER; MSG M9 (RUNNING)
       BRS 39; SKA =1B7; BRU S1; MSG M1 (MUST HAVE OP LIC ETC)
BELL   LDA =147B; LDX =-10; CIO =1; BRX *-1
       LDA =5000; BRS 81; BRU BELL

S1     LDA =BUF; LDX =100B; LDB =200B; BRS 124
       LDA BUF+75B; LRSH 12; SNE =1; BRU SUPLIC; SNE =4; BRU SUPLIC
       SNE =14; BRU SUPLIC; SNE =17; BRU SUPLIC; BRU MAPLIC
SUPLIC LDP SUPER; BRS 48; BRU NOSUP; LDB =FILBUF; LDX =10
       BRS 96; LDA FILBUF+3; ETR =6; SKE =6; BRU *+3
       MSG M7 (SUPER LIC OKAY)
       BRU MAPLIC; MSG M2 (SUPER HAS NO OR INCORRECT LIC)
       MSG M3 (SHUT SYST CONTACT SYSTEMS)
       BRU BELL
NOSUP  MSG M4 (NO NETWORK SUPER PRGM ON THIS SYSTEM)
MAPLIC LDP MAP; BRS 48; BRU NOMAP; LDB =FILBUF; LDX =10
       BRS 96; LDA FILBUF+3; ETR =6; SKE =6; BRU *+3
       MSG M8 (MAP LIC OKAY); BRU EOJ
       MSG M5 (MAP PROG HAS NO OR INCORRECT LIC)
       MSG M3 (SHUT SYSTEM ETC); BRU BELL
NOMAP  MSG M6 (MAP PROGRAM MISSING)
       MSG M3 (SHUT SYSTEM ETC); BRU BELL
EOJ    MSG MEOJ; BRS 10
MEOJ   ASC '$E.O.J.$/'

SUPFIL ASC '(SUP)SUPER'
SUPER  DATA 3*SUPFIL-1,3*SUPFIL-1+10
MAPFIL ASC '(MAP)MAP'
MAP    DATA 3*MAPFIL-1,3*MAPFIL-1+8

M1     ASC '$YOU MUST HAVE OPERATOR LICENSE SET TO PROCEED.'
       ASC '$SET LICENSE AND THEN IN THE EXEC TYPE: COM OPCOM'
       ASC '$TO COMPLETE SYSTEM BRING UP.  TYPE ALTMODE NOW TO'
       ASC '$STOP BELLS.$$/'
M2     ASC '$NETWORK SUPERVISOR PROGRAM  SUPER  HAS NO OR INCORRECT'
       ASC '$LICENSE.$/'
M3     ASC '$SHUT SYSTEM, AND CONTACT SYSTEMS PERSONNEL FOR CORRECTIVE'
       ASC '$ACTION.'
       ASC '$TYPE ALTMODE NOW TO STOP BELLS.$$/'
M4     ASC '$NO NETWORK SUPERVISOR PROGRAM ON THIS SYSTEM$/'
M5     ASC '$MAP PROGRAM HAS NO OR INCORRECT LICENSE.$/'
M6     ASC '$MAP PROGRAM MISSING$/'
M7     ASC '$NETWORK SUPERVISOR PROGRAM  SUPER  LICENSE OKAY$/'
M8     ASC '$MAP PROGRAM LICENSE OKAY$/'
M9     ASC ' RUNNING$/'
FILBUF BSS 50
BUF    BSS 100B
       END
1OPCK ** FICHE/FRAME BREAK *****
CKOP3 SYMSORT

SYM.  PG.LN. IDENT.

BELL    1 12 OPCK    BUF     2 17 OPCK    EOJ     1 32 OPCK    
FILBUF  2 16 OPCK    M1      2  1 OPCK    M2      2  5 OPCK    M3      2  7 OPCK    
M4      2 10 OPCK    M5      2 11 OPCK    M6      2 12 OPCK    M7      2 13 OPCK    
M8      2 14 OPCK    M9      2 15 OPCK    MAP     1 38 OPCK    MAPFIL  1 37 OPCK    
MAPLIC  1 25 OPCK    MEOJ    1 33 OPCK    MSG     1  7 OPCK    NOMAP   1 30 OPCK    
NOSUP   1 24 OPCK    S1      1 15 OPCK    START   1 10 OPCK    SUPER   1 36 OPCK    
SUPFIL  1 35 OPCK    SUPLIC  1 18 OPCK    VER     1  5 OPCK    
