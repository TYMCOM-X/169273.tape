; GENERAL-PURPOSE REGISTERS  (TOUCHED BY EVERYONE)
R0=0
R1=1
R2=2
;REGISTERS TOUCHED ONLY BY A,ACTRANS,BCTRANS
;R3-R5 USED IN CHECKSUMMING FILE (BEFORE TRANSMISSION)
R3=3            ;HOLDING REGISTER FOR C.  PRESERVE IN CW AND W MACROS
R4=4
RC=4          ;HOLDING REGISTER.  PRESERVE IN CW AND W MACROS.
R5=5
PREFER=5        ;PREFERRED COMPRESSION CHAR.
RCC=6            ;REGISTER USED FOR REPEAT COUNT
; SPECIAL PURPOSE REGISTERS
C=7             ;CHARACTER BEING READ(WRITTEN) FROM(TO) FILE OR BEING
;               WRITTEN(READ) TO(FROM) OUTPUT(INPUT) BUFFER.
RANK=10         ;RANK OF FIRST(SECOND) CHAR OF A MASTER-COMBINING
;               PAIR.  **MUST** FOLLOW C.
AS=11           ;STATE FOR ROUTINE A.
GS=12           ;STATE FOR ROUTINE GB.
GX=13           ;INDEX FOR ROUTINE GB.
PS=14           ;STATE FOR ROUTINE PB.
PX=15           ;INDEX FOR ROUTINE PB.
F=16            ;FLAGS
P=17            ;PUSHDOWN LIST POINTER
