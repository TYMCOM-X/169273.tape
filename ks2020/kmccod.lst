    866	000004					BRWRTE	IMM,	7		; Insure that there is no wrap around
    880	000006					SPBR	BR,	AANDB,	SP1
    895					;-----
    896					; Dispatch to input or output routines
    897					;-----
    898	000010					BR0	DO20			;If DEVICE is odd do 20 service
