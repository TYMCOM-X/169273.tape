TITLE BIONIC - TEST INPUT FROM 2780 USING NETWORK REMOTE BATCH   
REL==1    
VER==1    
LOC 137   
XWD  REL,VER   
RELOC
     
T1=1 
T2=2 
T3=3 
T4=4 
C=6  
FL==16    
     
FIL==1    
TTY==2    
;    
CASSET==10
D2780==13 
CRLFLG==10
DTAFLG==100    
CR==15    
LF==12    
;    
     
P=17 
     
EXTERNAL RDNAM,CMDLP,OUTDEC   
INTERNAL GETCHR,GORUN    
ARRAY OBUF[3],PDL[40],BIOBUF[^D101],INAM[5],FILNAM[15] 
ARRAY DEVDEF[3]
     
     
GETCHR: INCHWL C    
CPOPJ1: AOS (P)
        POPJ P,
     
STPT:   RESET  
        MOVE P,[IOWD 40,PDL]  
        PUSHJ P,CMDLP           ;GO SCAN FOR A COMMAND 
GORUN:  INIT FIL,2  
        SIXBIT /DSK/
        OBUF,,0
         JRST INIERR
        SETZ    FL, 
        SETZM   RECCNT#  
        SETZM   DEVDEF+2 
        MOVSI   T1,(SIXBIT/TTY/)   
        MOVEM   T1,DEVDEF+1   
        HRROI   T1,-24          ;GOING FOR LINE TYPE   
        GETTAB  T1, 
        JFCL   
        MOVSS   ,T1 
        LSH     T1,-6    
        ANDI    T1,77    
        SETZM   OUTDEV#  
        MOVEI   T2,CASSET       ;FOR CASSET TYPE MODE  
        CAIN    T1,13           ;CHECK FOR 2780 TYPE LINE   
        MOVEI   T2,D2780        ;GET 2780 TYPE MODE    
        MOVEM   T2,DEVDEF       ;STORE FOR OPEN   
        CAIN    T1,13           ;IS THIS 2780
        SETOM   OUTDEV          ;FLAG AS SUCH
        OPEN    TTY,DEVDEF      ;SET MODE KANA    
         JRST TTYERR
        OUTSTR [ASCIZ /OUTPUT TO FILE: /]    
        MOVEI T4,INAM    
        PUSHJ P,RDNAM    
         SKIPA 
        JFCL   
RDFIL:  MOVEI T1,15 
        MOVEM T1,FILNAM  
        MOVEI T1,INAM    
        SKIPE (T1)  
        MOVEM T1,FILNAM+1
        DMOVE T1,2(T4)   
        DMOVEM T1,FILNAM+2    
;       SET KANA FLAG    
        MOVSI 1,400 
        MOVEM 1,FILNAM+14
        ENTER FIL,FILNAM 
         JRST WRTERR
        SKIPN   OUTDEV   
        OUTCHI  21  
        MOVEI T2,5      ;SET INPUT BAUD RATE TO 1200   
        HRROI T1,15     ;AUXCAL DISPATCH ENTRY    
        SKIPE   OUTDEV  ;DONT SET BAUD FOR NON-2780    
        AUXCAL T1,T2    ;DO IT.    
        HRROI T1,34 
        AUXCAL T1,  
         JRST   [OUTSTR [ASCIZ @COULD NOT SET BLOCK I/O MODE FOR PORT.@]   
                EXIT]    
        OUTSTR  [ASCIZ/READY FOR INPUT  
/]   
        SKIPE   OUTDEV          ;SKIP IF NOT 2780 
        OUTSTR  [ASCIZ/  
     
     
     
/]   
        MOVEI T1,^D400   
        MOVEM T1,BIOBUF  
        HRROI T1,36 
        AUXCAL T1,BIOBUF        ;THIS SHOULD ALWAYS FAIL.   
        MOVE T1,[1,,10] ;10 SEC DELAY HACK FOR J KOPFJ 
        HIBER T1,   
         SKIPA 
        JFCL   
        MOVEI T1,1  
        MOVEM T1,PARITY# 
        HRROI T1,17     ;SET PARITY AS A SIGNAL TO THE 
        SKIPE   OUTDEV   
        AUXCAL T1,PARITY        ;REMOTE TO START 2780 PROTOCOL.  
LP:     MOVEI T1,^D400   
        MOVEM T1,BIOBUF  
        HRROI T1,35 
        AUXCAL T1,BIOBUF 
         JRST   [OUTSTR [ASCIZ /BLOCK INPUT ERROR./]   
                EXIT]    
        MOVE T1,[POINT 8,BIOBUF+1] 
        MOVEM T1,BIOPTR# 
              ;SKIP IF BLANK OR BETTER    
        JRST    DOCRLF          ;CHECK FOR CR OR LF    
        TRO     FL,DTAFLG       ;SAY WE HAVE DATA 
NOCR:   TRNE    FL,CRLFLG       ;SKIP IF FLAG OFF 
        JRST    NOCR2           ;MUST INSERT A LF 
NOCR1:  PUSHJ   P,DUMP          ;PUCH CHAR. TO FILE    
        JRST    LP2             ;GET NEXT CHAR.   
NOCR2:  PUSH    P,T1            ;SAVE CHAR.  
        MOVEI   T1,LF           ;INSERT LF AFTER CR    
        PUSHJ   P,DUMP          ;PUT IT TO FILE   
        POP     P,T1            ;RETURN WHATEVER  
        TRO     FL,DTAFLG       ;SET ON FOR RETURNED CHAR   
DONELF: TRZ     FL,CRLFLG       ;SAY DONE WITH CR, LF  
        AOS     RECCNT          ;ADD TO RECORD COUNT IN
        JRST    NOCR1           ;PROCESS CURRENT CHAR. 
DOCRLF: CAIN    T1,LF           ;LINE FEED?  
        JRST    DOLF            ;PROCESS IT  
        CAIE    T1,CR           ;CARRIAGE RETURN? 
        JRST    NOCR            ;NO, PROCESS NORMALLY  
        TRZ     FL,DTAFLG       ;TURN DATA FLAG OFF    
        TRON    FL,CRLFLG       ;SEE IF CR AFTER CR    
        JRST    NOCR1           ;NO, THEN ON WE GO
        MOVEI   T1,LF        ,CR         JR ; AS BEFORFL,DTAFLGER DATA  ELF      B  
LPE
         CRLFLG     LF YET  P         ;DONE, SO GO ON   
        MOVEI   T1,LF           ;GET THAT LF 
