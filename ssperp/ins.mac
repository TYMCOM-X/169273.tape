        addm	r1,hrs			;now it's corrected to GMT
	move	r2,hrs			;put it in an AC for checking
        CAIL    R2,^D24			;see if hr is geq 24
        JRST    SLOW			;yep--go fix up hrs and days
        skipge  r2
