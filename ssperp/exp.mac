;TAKES BINARY DATE AND CONVERTS TO MONTH DAY YEAR
FDMN::  Z
        SETZ    R3,
        CAMG    R1,YTB+1(R3)
        JRST    .+3
        AOS     R3
        JRST    .-3
        MOVEM   R3,YR
        SUB     R1,YTB(R3)
	trnn	r3,3		;test to see if year divisible by 4
	jrst	FdmnLy		;yes, so use month table for leap years
        SETZ    R3,
        CAMG    R1,MTB+1(R3)
        JRST    .+3
        AOS     R3
        JRST    .-3
        MOVEM   R3,MN
        SUB     R1,MTB(R3)
        JRST    fdmnrt
fdmnly: setz	r3,
	camg	r1,MtbLy+1(r3)
        jrst    .+3
	aos	r3	
	jrst	.-3	
        movem   r3,mn
        sub     r1,MtbLy(r3)
fdmnrt: MOVEM   R1,DY
        JRST    @FDMN

;TAKES YR MN DY AND CONVERTS BACK TO BINARY
GBNDT:  Z
        SKIPL   YR
        JRST    .+3
        SETZ    R1,
        JRST   @GBNDT
	move	r3,mn
	move	r1,yr
	trne	r1,3
	jrst	.+3		; not a leap year so use regular table
	move	r1,MtbLy(r3)
	skipa
	move	r1,mtb(r3)
	move	r3,yr
	add	r1,ytb(r3)	; add in no. of days up to begin of year
        ADD     R1,DY		; add in no. of days since 1st of month
        JRST    @GBNDT

;CONVERTS DATE WORD FROM TO BINARY FORM

DATCV:  MOVEM   R1,T1
        LSHC    R1,-11
        ANDI    R1,17
        MOVE    R3,R1
        MOVE    R1,YTB(R3)
        MOVEM   R1,T2
	move	r1,r3
	trne	r1,3
	jrst	datcv1		;not a leap year so use regular table
        SETZ    R1,
        LSHC    R1,4
        MOVE    R3,R1
        MOVE    R1,MTBly(R3)
	jrst	datcv2
datcv1: setz	r1,
	lshc	r1,4
	move	r3,r1
	move	r1,mtb(r3)
datcv2: ADDM    R1,T2
        SETZ    R1,
        LSHC    R1,5
        ADD     R1,T2
        POPJ    P,

GCRDT:  DATE    R1,     ;READ DATE TO R1
        PUSHJ   P,GCRDTS
        SOS     R1
        SOS     T3
        POPJ    P,
GCRDTS: IDIVI   R1,^D31
        AOS     R2
        MOVEM   R2,DY
        IDIVI   R1,^D12
        MOVEM   R2,MN
        SUBI    R1,10
        MOVE    R3,YTB(R1)
	trne	r1,3		;skip if this IS a leap year
	jrst	.+3
	add	r3,MtbLy(r2)
	skipa
        ADD     R3,MTB(R2)
        ADD     R3,DY
        MOVE    R1,R3
        MOVEM   R1,T3
        POPJ    P,

