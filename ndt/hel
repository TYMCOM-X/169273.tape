	FCALL APPLY @E )  P	EXEC EVLLST TYPIRG	SCAN		XID  
	SPROUT APPLY @E )  SPROUT 
				EXEC SAPPL		SCAN		FK3 #Q100

MUMBLE sprout;
FK:	SPROUT (  SPROUT	EXEC INMAIN		SCAN	^EX	FK1  #XXQQ
FK1:	SPROUT @E ,  SPROUT	EXEC BNDITM		SCAN 2		FK2  #Q97 
