	PCALL APPLY )  S 	EXEC EVLNLL		SCAN 		S9   
	SPROUT APPLY )  SPROUT 
				EXEC SAPPN		SCAN		FK3 #XXQQ
%EV4:   APPLY @E )  APPLY 	EXEC BNDITM PITSTK	SCAN		%EV3 #Q99
