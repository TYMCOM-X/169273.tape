FL2:	 @DOCA 		EXEC DFSET SETDLM	SCAN	^CONX	FL3
FL3:	 FLDPL SG ENDC  FLDPL EXEC DFENT FFPOP OFFDLM SETFL CPOPBT CPOPET POPLST 
							PRESUME		FL4
