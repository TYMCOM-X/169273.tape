				EXEC FPREM RDYCAL	SCAN	^EX	PARM
	SPROUT IPR SG  SPROUT SG 
				EXEC FPREM PDASTK			FK3  
	SPROUT APPLY (  SPROUT APPLY 
				EXEC SAPPL1		SCAN 2		%EV2 #XXQQ
FK3:	, 						SCAN		FK4
	SPROUT )  S 		EXEC STDOPT STDKLL SPRIT 
							SCAN		S9   #XXQQ
