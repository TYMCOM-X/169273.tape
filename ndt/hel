	 E MESSAGE 		EXEC TYPMSG		SCAN		RQ1 #Q10
RQ1:	 REQUIRE ; 		EXEC CLRSET		SCAN 	
	 REQUIRE ,  REQUIRE				SCAN  		RQQ  #Q10

RQ3:	 REQUIRE E SOURCEFILE ,  REQUIRE 
				EXEC REQERR		SCAN  		RQQ
	 REQUIRE E SOURCEFILE ;  
				EXEC SRCSWT		SCAN		RQ4  #Q11
