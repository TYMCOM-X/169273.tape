ENTRY SETXP;
TITLE SETXP
	SEARCH SLBPRE
	PREAMBLE
SETXP: ;internal real simple procedure setxp( real x; integer exp );
DEFINE X,<-2(P)>
DEFINE EXP,<-1(P)>
EXTERNAL X33
	MOVM	A,X;
	JUMPLE	A,RETURN;
	MOVE	TEMP,EXP;
	ADDI	TEMP,=128;
	DPB	TEMP,[POINT 8,A,8];
	SKIPGE	X;
	 MOVN	A,A; 
RETURN:
	SUB	P,X33
	JRST	@3(P)
PRGEND
