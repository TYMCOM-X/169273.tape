;
	PUSH	P,VAL	;push the arg off and call DFINT
	PUSH	P,VAL
	PUSHJ	P,DFINT
	CAMN	A,VAL
	 CAML	A+1,VAL.LO
	  CAMGE	A,VAL
	   DFAD	A,[1.0
		   0]
