begin
    integer B, NLPOS;
    string SBUF;		! working buffer;
    string XTEXT;		! expanded text;
    XTEXT := null;
    NLPOS := length( SBUF := BUF );
    while length( BUF ) do begin
	scan( BUF, tabBrk, B );
	case B of begin	
	    [#ht] begin
		integer I;
