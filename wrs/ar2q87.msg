    mai:RCPT[M] := mai:Date[M] := mai:Bcc[M] := null;
    S := maiMMake( M );
    while length(S) do begin
	string REC;
	integer B;
	REC := tabExpand( scan( S, lineBrk, B ));
	if equ(REC,".") then REC := "..";
