/*
 * rkCrash
 *
 * Logger routine to log a crash code and then trigger a boot.
 */
void
rkCrash( productCode, crashCode )
char crashCode[];
char productCode[];
{
	k_disableInterrupts();			/* kernel */
	if( glob.status == ST_CRASH )
 		k_reset( 3 );			/* kernel (nested crash) */
	glob.status = ST_CRASH;
/*
 * b_saveRegs() is implemented by the boot.  It saves all device registers
 * in static memory for inclusion in the dump if one is required.  This
 * is called only by k_crash and with interrupts disabled.
 */
	b_saveRegs();
/*
 * k_logCrash() is implemented in the logger.  It first saves its arguments
 * in glob.crashCode, saves the current glob.utc in glob.crashUtc and then
 * builds a message to log the crash and puts it on the queue.  This routine
 * is called only by the kernel and with interrupts disabled.
 */
	l_logCrash( productCode, crashCode );
/*
 * k_reset() is implemented by the kernel.  Argument of 1 causes a warm start
 * leaving all static data structures intact.
 */
	k_reset( 1 );				/* kernel */
}


/*************************************************************************
	These routines are needed by rkCrash()
*************************************************************************/


void
k_reset( type )
int type;
{
	if( type == 0 ) return;
/*
 * k_disableInterrupts() is a kernel routine to stop all concurrent activity
 * on the board.  Non-maskable interrupts are dealt with by an immediate
 * dismis if they can't be turned off.
 */
	k_disableInterrupts();
/*
 * l_flush() is implemented by the logger.  It should try to write out any
 * queued log messages using the mindlessly simple IPCF routines since
 * interrupts are disabled.  It should not return until this either works
 * or appears hopeless.  l_flush() is called only by k_reset() and with
 * interrupts disabled.
 */
	l_flush();

	switch( type ) {
	case 2:
		zeroPatronFlag = TRUE;
		break;
	case 3:
		glob.magic = glob.magic1c = 0;
		break;
	}
/*
 * This register causes the board to be hard reset as if by the reset button.
 * This is stronger than the RESET instruction since all on-board devices are
 * affected including the CPU.
 */
	boardHardResetRegister++;
/*
 * This should never be executed.  It halts the CPU as a last resort.
 */
	haltCpu();
}


void
b_saveRegs()
{
	/* save hardware registers in static area for possible dump */
	/* interrupts are disabled */
}


void
l_flush()
{
	/* flush the buffer */
	/* interrupts are disabled */
	/* use mindlessly simple ipcf routines */
}


/*************************************************************************
	These are the mindlessly simple IPCF routines.
*************************************************************************/

/*
 * k_simpleSend
 *
 * Mindlessly stupid send does no checking or anything - just works.
 */
void
k_simpleSend( msgptr );
struct IpcfHdr *msgptr;

/*
 * k_simpleRead
 *
 * Mindlessly stupid read does no checking or anything - just works.
 * Uses no interrupts or timeouts.  Returns the next message put in
 * the card's ring regardless of who its to (beware).  Also user
 * needs to check the ipcfControl bit, too.  Returns NULL if no message.
 */
struct IpcfHdr *
k_simpleRead();
