63	define ForDef(Group,NameList) = {
64	    ReDefine Group = 0;
65	    ReDefine ForDefX = {
66		ReDefine Group}&{$Str = {};
67		ForLC $Name$_ (}&{NameList}&{)
68		 doC { !Block(Group,$Name$)
69		       redefine Group}&{$Str = cvms(Group}&{$Str) &
70			       ifcr length(cvms(Group}&{$Str)) thenc {,} & endc
71			        { "$Name$" };
72		     }
73		 endC
74	    };
75	    ForDefX
76	};
