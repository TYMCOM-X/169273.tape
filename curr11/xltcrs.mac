;TABLE USED WHEN OUTPUTTING CHARACTER PAIRS FOR ASCII CHARACTER WHICH DO NOT
;  HAVE A GOOD CORRESPONDENCE EQUIVALENT - INDEX IS 6 BIT VALUE OBTAINED FROM
;  THE ASCII TO CORRESPONDENCE TABLE WHEN BOTH 200 BIT IS CLEAR AND 100 BIT IS
;  SET

	.MACRO	XC  SYM,CNTS
XC'SYM=!.N
.N=!.N+1
	.BYTE	CNTS
	.ENDM
.N=!100

XCXCRS::XC	SLH,'/		; \
	XC	UPA,'@		; ^
	XC	SQT,''		; GRAVE
	XC	PLS,'+		; LEFT BRACE
	XC	PER,'!		; VERTICAL BAR
	XC	AST,'*		; RIGHT BRACE
	XC	TLD,'"		; TILDE

	.EVEN
