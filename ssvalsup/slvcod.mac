SUBTTL BITDEF - slave bit definitions

; ***** cud license bits  (CPARW)
TYM.CD==(1B12)  ; tymshare license in cud
PRM.CD==(1B13)  ; premium products
OPR.CD==(1B14)  ; operator license in cud
LOS.CD==(1B15)  ; login over shut
AS.CD== (1B16)  ; acct sup in cud
HSH.CD==(1B17)  ; hushlog lic in cud
NML.CD==1B18    ; no multiple login in cud
SLW.CD==1B20    ; slow user
NPC.CD==1B21    ; no change password
GTW.CD==1B22    ; transparent gateway
RUB.CD==1B23    ; rub bit in cud

; **** bits in colon word (NMCOLN)
BIL.CD==(1B12)  ; billable bit in cud
CLN.CD==(1B13)  ; colon option
NPS.CD==(1B15)  ; no password bit

; **** bits for class items
GRP.CD==1B21    ; this is a group
HOM.CD==1B20    ; this is a home
UDF.CD==1B24    ; class is self defined
UDN.CD==1B25    ; if class is self defined, this makes it negative.
HOS.CD==1B20    ; if class is self defined, this is a host, not a node.

; updlud bits
; privilege bits
NOL.LX==(1B0)   ; user may not login under this name
HSH.LX==(1B1)   ; hush
RUB.LX==(1B2)   ; restricted user
GFD.LX==(1B3)   ; all in gan may gfd here
BUD.LX==(1B4)   ; user is controlled
TMO.LX==(1B14)  ; timeout after disconnect
DET.LX==(1B15)  ; detach on disconnect
TYM.LX==(1B16)  ; tymshare license
AS.LX== (1B17)  ; acct sup

BIL.LX==1B18    ; billable
RCM.LX==1B21    ; restricted command mode
MOD.LX==POINT 2,0,23   ; pointer for executive mode
PDP.LX==0       ; pdp mode
XEX.LX==1       ; xexec mode
XEH.LX==2       ; xexec-j mode
SUD.LX==3       ; suds mode

; **** bits in misc bits word for move users (tymcom-10)
BUD.MI==10	; budget word present
INF.MI==1	; init file info present

; **** masks and shifts for 940 slave (list user lud cmd)
GANMSK==377777          ; mask off gan
UUNMSK==GANMSK          ; mask uun
DISMSK==1774            ; mask for dist lsh 2
DISSHF==-2              ; shift for district
TZSHFT==-^D16           ; shift for timezone

        END
