Date: Tue, 28 Jan 86 14:04:45 PST 
To: Carl A Baltrunas<Carl@X930.Tymnet> 
Subject: here's the code. 

!! title Rtn Contains ;
! =========================================================================
=====================================+=====================================
=                                                                         =
=                                                                         =
=                              Rtn Contains                               =
=                                                                         =
=                                                                         =
=====================================+=====================================


        Look for the presence of the string Tst in the string Src,
        using the appropriate comparison.  We loosely base this code
        on the "kequ" routine from the SAIL runtimes written in FAIL.

 
==========================================================================;


  internal simple boolean procedure Contains (string Src, Tst;
      boolean Exactly(FALSE));
  begin "Contains"
    integer
      SrvLen,
      SrvPtr;

    start!code
      define
        CASEBIT   = '40,
        R1        = '01,
        R2        = '02,
        R3        = '03,
        R4        = '04,
        SrcLen    = '01,
        SrcPtr    = '02,
        TstLen    = '03,
        TstPtr    = '04,
        Ch1       = '13,
        Ch2       = '14,
        MatchCase = '15,
        Sp        = '16,
        P         = '17;

      label
        A1,
        B1,
        B2,
        C1,
        D1,
        D2,
        E1,
        TrueRet,
        FalseRet;


        move      MatchCase, Exactly;
        push      P, R2;
        push      P, R3;
        push      P, R4;

        hrrz      TstLen, -1(Sp);
        skipg     TstLen;
        jrst      FalseRet;
        hrrz      SrcLen, -3(Sp);
        camge     SrcLen, TstLen;
        jrst      FalseRet;
        move      SrcPtr, -2(Sp);
        ibp       SrcPtr;

      A1:
        jumple    SrcLen, FalseRet;
        move      TstPtr, 0(Sp);
        ibp       TstPtr;
        hrrz      TstLen, -1(Sp);

      B1:
        camge     SrcLen, TstLen;
        jrst      C1;
        jumple    SrcLen, C1;
        ldb       Ch1, SrcPtr;
        ldb       Ch2, TstPtr;
        xor       Ch1, Ch2;
        jumpe     Ch1, C1;
        jumpn     MatchCase, B2;
        caie      Ch1, CASEBIT;
        jrst      B2;
        trz       Ch2, CASEBIT;
        cail      Ch2, "A";
        caile     Ch2, "Z";
        skipa;
        jrst      C1;

      B2:
        ibp       SrcPtr;
        soja      SrcLen, B1;
        
      C1:
        camge     SrcLen, TstLen;
        jrst      FalseRet;
        jumple    SrcLen, FalseRet;
        movem     SrcPtr, SrvPtr;
        movem     SrcLen, SrvLen;

      D1:
        jumple    TstLen, E1;
        ldb       Ch1, SrcPtr;
        ldb       Ch2, TstPtr;
        xor       Ch1, Ch2;
        jumpe     Ch1, D2;
        jumpn     MatchCase, E1;
        caie      Ch1, CASEBIT; 
        jrst      E1;
        trz       Ch2, CASEBIT;
        cail      Ch2, "A";
        caile     Ch2, "Z";
        jrst      E1;

      D2:
        ibp       SrcPtr;
        ibp       TstPtr;
        soj       SrcLen,;
        soja      TstLen, D1;

      E1:
        jumpe     TstLen, TrueRet;
        move      SrcPtr, SrvPtr;
        ibp       SrcPtr;
        move      SrcLen, SrvLen;
        soja      SrcLen, A1;

      FalseRet:
        setz      R1,;
        skipa;

      TrueRet:
        seto      R1,;

        pop       P, R4;
        pop       P, R3;
        pop       P, R2;
    end;
  end "Contains";


! ===================================+=====================================
==========================================================================;

