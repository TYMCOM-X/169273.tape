	T1=6
	T2=T1+1
	T3=T1+2
	T4=T1+3

	LF=12
	CR=15

	DEFINE	TYPE(TEXT)<
	SALL			;;DON'T LET IT SHOW
	OUTSTR	[ASCIZ	TEXT]	;;LET THE PROGRAMMER SUPPLY DELIMs
	OUTSTR	[BYTE	(7)15,12,0,0,0]	;;DO CRLF
	XALL			;;BACK TO NORMAL LISTING MODE
	>
DEFINE	BOMB(TEXT)<
	SALL
	JRST	[CLRBFI
		CLOSE	0
		CLOSE	1
		TYPE	TEXT
		EXIT
		]
	XALL>
DEFINE	CAIJE(AC,E,ADDR)<
	SALL
	CAIN	AC,E		;;DON'T  JUMP IF NOT EQUAL
	  JRST	ADDR
	XALL>
DEFINE	OTCRLF<SALL
	OUTSTR [ASCIZ /
/]
	XALL>

OPDEF	CALL	[PUSHJ	P,0]
OPDEF	RET	[POPJ	P,0]

.AXOST==52	;AUXCAL OUTSTR function
START:
	TDZA
	HRLZI	1
	RESET
	JUMPN	NOPRNT
	MOVE	P,PDL
	CALL OUTV
NOPRNT:	HRRI	[SIXBIT /SYS/
		 SIXBIT /LOGOUT/
		0
		0
		1,,4
		0]
	RUN
	HALT
	EXIT

OUTV:	mstime	T1,
	ADD	T1,LASTC
;;;	lsh	T1,-4	;clear out low order noise
	MOVE	T2,TABLEN
	idivi	T1,(T2)
	ADDM	T1,LASTC
	ADDM	T2,LASTC
	HRRZ	T2,TABLE(T2)
	MOVEI	T3,1(T2)	;SKIP THE BCV NUMBER
	OTCRLF
	OUTSTR	@T3
	OUTSTR	[ASCIZ /
							-- /]
	MOVE	T1,(T2)		;GET BCV NUM
	CALL PUTBCV
	OTCRLF
	RET
	EXIT


PUTBCV:	MOVE T2,T1	;SAVE BCV
	LSH T1,-^D27	;HIGH 9 BITS
	MOVE	T1,BOOK6-1(T1)		;GET SIXBIT BOOK NAME
	CALL	O6BIT
	HLRZ	T1,T2
	ANDI	T1,377
	CALL PUTNUM
	OUTSTR [ASCIZ /:/]
	MOVE	T1,T2
	ANDI	T1,377
	CALL PUTNUM
	RET

O6BIT:	PUSH	P,T1
	PUSH	P,T2
	PUSH	P,T3
	MOVE	T3,[POINT 7,O6TEXT]
O6LOOP:	MOVE	T2,T1
	LSH	T1,6
	LSH	T2,-^D30
	ADDI	T2," "		;CONVERT TO ASCII
	IDPB	T2,T3
	SUBI	T2," "
	JUMPN	T2,O6LOOP	;IF NOT A 0 CONTINUE
	IDPB	T2,T3
	OUTSTR	O6TEXT
	POP	P,T3
	POP	P,T2
	POP	P,T1
	RET
O6TEXT:	ASCIZ /123456 /

PUTNUM:	PUSH	P,T1
	PUSH	P,T2
	PUSH	P,T3
	CALL	SPEW10
	POP	P,T3
	POP	P,T2
	POP	P,T1
	RET

SPEW10:	MOVEI	T3,^D10
	SKIPA
SPEW8:	MOVEI	T3,^D8
SPEW:	IDIV	T1,T3		;Divide by radix 
	HRLM	T2,(P)		;Keep remainder as digit
	SKIPE	T1		;If quotient zero, done ...
	CALL	SPEW		;...else do it again
	HLRZ	T1,(P)		;Recover digit
	ADDI	T1,"0"		;Add ASCII bias
	OUTCHR	T1		;Send to TTY
	RET			;Do next until return

CPOPJ1:	AOS (P)
CPOPJ:	RET
PDL:	XWD -PDLLEN,PDL+1
	BLOCK 30
	PDLLEN==.-PDL-1
LASTC:	0


