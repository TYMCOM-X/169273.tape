	MOVEI	AC1,377				; AC1,377									0517
	CAMN	AC1,U.2				; AC1,TEXT%COUNT
	PUSHJ	SP,TT%OUTPUT			; SP,TT%OUTPUT
	POPJ	SP,				; SP,										0462

