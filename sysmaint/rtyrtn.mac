        ADDM    1,0(STK)
        LDB     1,[ POINT 10,0(6),9 ]
        ADDI    1,S 0
        MOVE    1,0(1)
        MOVE    2,1(6)
        LDB     3,[ POINT 4,0(6),13 ]
        HRRZ    4,0(6)
        LDB     5,[ POINT 4,0(6),17 ]
