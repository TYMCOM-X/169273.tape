        HLRZ    1,DAYMTH-1(1)   ;LOAD LEFT HALF (-1 TO RANGE 0-11)
        CAIE    1,28            ;IS IT FEBRUARY (28 DAYS)
        POPJ    STK,            ;NO, SO RETURN
