        MOVE    2, [ POINT 7,S (STR-1),35 ]
        PUSHJ   STK,CNTSC
        MOVE    2, [ POINT 7,S (STR-1),35 ]
        POP     STK,5           ;RESTORE MAX LENGTH
        POP     STK,4           ;RESTORE BYTE POINTER
        CAMLE   1,5             ;COMPARE CHAR COUNT TO MAX LENGTH
        PUSHJ   STK,[ MOVE 1,5
                      MOVE 2, [ POINT 7, [ ASCII "***************" ] ]
                      POPJ STK, ]
        SUB     5,1             ;COMPUTE NUMBER OF LEADING BLANKS
        MOVEI   3," "           ;CHAR 'BLANK'
