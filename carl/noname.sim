DEF N.O.LST.CMDS AS 5	!NUMBER OF OPER-ONLY LIST COMMANDS
DEF N.CMDS AS 8	!NUMBER OF PRIMARY COMMANDS
DEF N.TYPES AS 2
DEF N.MODES AS 4	!NUMBER OF MODES
