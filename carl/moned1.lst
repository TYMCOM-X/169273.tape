47	recursive string procedure INCR(string S);
48	! incriment the first letter of string S...skip I and O;
49	begin	"INCR"
50	    integer C;
51	    case C _ lop(S) of begin
52		[0] return( "A" );
53		["I"-1] return( ("I"+1) & S );
54		["O"-1] return( ("O"+1) & S );
55		["Z"] return( "A" & INCR(S) );
56		else return( (C+1) & S )
57	    end;
58	end	"INCR";
