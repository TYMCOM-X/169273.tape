Begin "JQUE"

require "(SAILIB)SAIL.DEF"   source!file;
require "(SAILIB)UUOSYM.DEF" source!file;
require "(CARL)JQUE.DEF"     source!file;
require "(CARL)JQUEUE.DEF"   source!file;
