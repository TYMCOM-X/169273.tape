	XWD	-3,CTLOC
	XWD	-3,CTLOC+3
	XWD	-3,CTLOC+6
	XWD	-2,DTLOC
	XWD	-2,DTLOC+2
	XWD	-2,DTLOC+4
	XWD	-2,DTLOC+6
	XWD	-1,PTLOC
	XWD	-1,PTLOC+1
	XWD	-1,PTLOC+2
	XWD	-1,PTLOC+3
	XWD	-1,PTLOC+4
LOCLEN==.-LOCTBL		;LENGTH OF SHIPS LIST
 
SNKTBL:	"P",,PSHP
	"D",,DSHP
	"C",,CSHP
	"B",,BSHP
	"A",,ASHP
 
