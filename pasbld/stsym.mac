	subttl	$CONCAT
;
; $CONCAT macro generates modules called directly by compiler-generated
; code to concatenate to the end of a string. The target (and first operand
; of the concatenation) may be F or X format, while the second string
; operand may be F, X, or C format. Upper/lower casing and padding with
; blanks is optional.
;
; All code required for a C format source is emitted directly. X and F
; format source strings are passed to the more general routines generated
; by the $CAT macro if extended string instructions are not
; available; otherwise all registers are set up and passed
; along to the routines generated by $EXMOVESTRING.
;
; The calling sequence and argument list is:
;	PUSHJ	TOP,routine
;	ARG	0,addr(dest)	; to byte pointer if X
;	ARG	0,maxlen	; of target
;	ARG	0,length	; current length of target
;	ARG	0,addr(source)	; to byte pointer if X
;	arg	0,length	; of source if not C format
;

	define	$concat (dest,source,pad,case),<
	entry	C'pad''case'.'dest''source'

	ifidn	<source><C>,<
	len==4>
	ifdif	<source><C>,<
	len==5
	ifn	$extend,<
	$temp	rsave,5
	$temp	srclen,1>>
C'pad''case'.'dest''source':	movei	r1,len
	addb	r1,0(top)	; bump return addr past argument list
	ifidn	<dest><F>,<
	movei	r0,@-len(r1)	; get addr of target string
	hrli	r0,440700>	; make a byte pointer
	ifidn	<dest><X>,<
	move	r0,@-len(r1)>	; get target byte pointer
	movem	r0,$str(pda)
	ifidn	<source><C>,<
	movei	r0,@-len+1(r1)	; get target maxlength
	subi	r0,@-len+2(r1)	; find remainder length
	movem	r0,$strlen(pda)
	move	r0,@-1(r1)	; get char
	$ulcase	(r0,case)
	movei	r1,@-2(r1)	; get current length
	adjbp	r1,$str(pda)
	sosl	$strlen(pda)
	idpb	r0,r1
	movem	r1,$str(pda)
	ifidn	<pad><P>,<
	jrst	r.pad##>	; complete padding
	ifdif	<pad><P>,<
	popj	top,>>
	ifdif	<source><C>,<
	ife	$extend,<	; if not extended instructions
	ifidn	<source><X>,<
	move	r0,@-2(r1)	; get source byte pointer
	jrst	CX.'pad''case'##>
	ifidn	<source><F>,<
	jrst	CF.'pad''case'##>>
	ifn	$extend,<	; use extended instruction set
	$save	rsave,r1+1,r1+5
	movei	r0,@-4(r1)	; get maxlen
	subi	r0,@-3(r1)	; calculate remainder length
	movem	r0,$strlen(pda)
	jumple	r0,ms.fin##	; done if string full
	movei	r0,@-1(r1)	; get source length
	movem	r0,srclen	; and save it
	ifidn	<source><X>,<
	move	r0,@-2(r1)>	; get source byte pointer
	ifidn	<source><F>,<
	movei	r0,@-2(r1)	; get source address
	hrli	r0,440700>	; make into byte pointer
	movei	r1+4,@-3(r1)	; get current destination length
	move	r1+3,$strlen(pda); retrieve remainder length
	adjbp	r1+4,$str(pda)	; update dest byte pointer
	move	r1+1,r0		; move source byte pointer 
	move	r1,srclen	; retrieve source length
	jrst	ms.'pad''case'##>>>	; go do move
	page
