loop:	sojl	r1,srcdone>
	ifdif	<pad><P>,<
loop:	sojl	r1,done>
loop2:	sosge	$strlen(pda)
	jrst	done
	ildb	exp1,r0
	$ulcase(exp1,case)
	idpb	exp1,$str(pda)
	sojge	r1,loop2
	ifidn	<pad><P>,<
srcdone:move	exp1,rsave
	jrst	r.pad##>
done:	move	exp1,rsave
	popj	top,
	ifdif	<case><L>,<
	ifdif	<case><U>,<
same:	camle	r1,$strlen(pda)
	move	r1,$strlen(pda)
	movn	r0,r1
	adjbp	r1,$str(pda)
	movem	r1,$str(pda)
	addm	r0,$strlen(pda)
	ifidn	<pad><P>,<
	jrst	r.pad##>
	ifdif	<pad><P>,<
