    end
    else select .tp of nset
	"P":	return sixbit 'HP2621';
	"K":	return sixbit 'H19   ';
	"?H":	return sixbit 'H1500 ';
	"H":	return sixbit 'H2000 ';
	"V":	return sixbit 'vt52  ';
	"?V":	return sixbit 'vt100 ';
	"C":	return sixbit 'concep';
	"?A":	return sixbit 'adm31 ';
	"A":	return sixbit 'adm   ';
