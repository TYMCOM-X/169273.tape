	Aoja	v,Cpopj			;	 + 1

UPDCUR:	Caile	t," "			; else if .xc geq " "
	Caile	t,177			;     and .xc lss .Rub
	  Caia				; NO
	Aoja	v,Cpopj			;	then .foo + 1
	Camn	t,ESCVALUE		; else if .xc eql .EscValue
